BottomText.Size = UDim2.new(1, -10, 1, 0)
BottomText.Position = UDim2.new(0, 10, 0, 0)
BottomText.BackgroundTransparency = 1
BottomText.Text = ">> PROTECTION: OBFUSCATED | STATUS: ENCRYPTED <<"
BottomText.TextColor3 = Color3.fromRGB(100, 255, 100)
BottomText.TextSize = 12
BottomText.Font = Enum.Font.Code
BottomText.TextXAlignment = Enum.TextXAlignment.Left
BottomText.Parent = BottomBar

--// Audio & State Management
local bgSound = nil
local isPlaying = false

local function playAudio()
    if not isPlaying then
        isPlaying = true
        bgSound = createSound()
        bgSound:Play()
        typewriter(Console, ">> AUDIO COUNTERMEASURES DEPLOYED\n>> TRACK ID: " .. SONG_ID, 0.03)
    end
end

local function stopAudio()
    if bgSound then
        bgSound:Stop()
        bgSound:Destroy()
        isPlaying = false
    end
end

--// Initialization Sequence
spawn(function()
    wait(0.5)
    typewriter(Console, ">> INITIALIZING CONGLOMERATE PROTOCOL...\n>> HINT: " .. HINT_TEXT .. "\n>> AWAITING ACCESS KEY", 0.04)
    playAudio()
end)

--// Countdown Engine
local timeRemaining = COUNTDOWN_TIME
local countdownRunning = true

spawn(function()
    while countdownRunning and timeRemaining > 0 do
        wait(1)
        timeRemaining = timeRemaining - 1
        Countdown.Text = string.format("AUTO-BYPASS IN: %ds", timeRemaining)
        
        --// Critical Warning Effect
        if timeRemaining <= 10 then
            Countdown.TextColor3 = Color3.fromRGB(255, 0, 0)
            wait(0.2)
            Countdown.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end
    
    if countdownRunning then
        countdownRunning = false
        stopAudio()
        TweenService:Create(MainFrame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(0, 30, 0)}):Play()
        Console.TextColor3 = Color3.fromRGB(0, 255, 0)
        typewriter(Console, ">> TEMPORAL BYPASS SUCCESSFUL!\n>> ACCESS GRANTED BY PATIENCE\n>> WELCOME, OPERATIVE", 0.03)
        wait(2)
        ScreenGui:Destroy()
        warn("\n[λ] SYSTEM UNLOCKED - EXECUTE MAIN PAYLOAD\n[λ] CONGLOMERATE PROTOCOL TERMINATED\n")
    end
end)

--// Key Validation Engine
local function checkKey(key)
    return tostring(key):lower() == KEY_1 or tostring(key):lower() == KEY_2
end

--// Submit Handler
SubmitBtn.MouseButton1Click:Connect(function()
    local key = KeyInput.Text
    if checkKey(key) then
        countdownRunning = false
        stopAudio()
        TweenService:Create(MainFrame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(0, 30, 0)}):Play()
        Console.TextColor3 = Color3.fromRGB(0, 255, 0)
        typewriter(Console, ">> VALID KEY CONFIRMED: " .. key:upper() .. "\n>> BYPASSING SECURITY LAYERS...\n>> ACCESS GRANTED!", 0.03)
        wait(2)
        ScreenGui:Destroy()
        warn("\n[λ] SYSTEM UNLOCKED - EXECUTE MAIN PAYLOAD\n[λ] CONGLOMERATE PROTOCOL TERMINATED\n")
    else
        Console.TextColor3 = Color3.fromRGB(255, 0, 0)
        typewriter(Console, ">> INVALID KEY: " .. key .. "\n>> ATTEMPT LOGGED\n>> AWAITING AUTO-BYPASS...", 0.02)
        KeyInput.Text = ""
        TweenService:Create(KeyInput, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 0, 0)}):Play()
        wait(0.5)
        TweenService:Create(KeyInput, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(25, 25, 25)}):Play()
    end
end)

KeyInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then SubmitBtn:Click() end
end)

--// Hover Effects
SubmitBtn.MouseEnter:Connect(function()
    TweenService:Create(SubmitBtn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 140, 255)}):Play()
end)

SubmitBtn.MouseLeave:Connect(function()
    TweenService:Create(SubmitBtn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 100, 255)}):Play()
end)

--// Draggable Interface
local drag, startPos, startMouse = false, nil, nil

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        drag = true
        startPos = MainFrame.Position
        startMouse = input.Position
    end
end)

TopBar.InputChanged:Connect(function(input)
    if drag and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - startMouse
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

TopBar.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then drag = false end
end)

--// Glitch Effect Engine
spawn(function()
    while ScreenGui.Parent do
        wait(0.7)
        if math.random() > 0.85 then
            BottomText.Text = ">> PROTECTION: " .. string.rep("*", math.random(8, 20)) .. " | STATUS: " .. string.upper(string.char(math.random(65, 90)) .. math.random(1000, 9999))
            wait(0.1)
            BottomText.Text = ">> PROTECTION: OBFUSCATED | STATUS: ENCRYPTED <<"
        end
    end
end)

print("\n[λ] CONGLOMERATE PROTOCOL ACTIVE")
print("[λ] AWAITING OPERATOR INPUT...\n")
