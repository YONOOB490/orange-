SubmitBtn.Position = UDim2.new(0, 10, 0, 195)
SubmitBtn.BackgroundColor3 = Color3.fromRGB(0, 100, 255)
SubmitBtn.BorderSizePixel = 0
SubmitBtn.Text = "[λ] INITIATE BYPASS SEQUENCE"
SubmitBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
SubmitBtn.TextSize = 18
SubmitBtn.Font = Enum.Font.Code
SubmitBtn.Parent = Content
Instance.new("UICorner", SubmitBtn).CornerRadius = UDim.new(0, 6)

--// Countdown Timer
local Countdown = Instance.new("TextLabel")
Countdown.Size = UDim2.new(1, -20, 0, 35)
Countdown.Position = UDim2.new(0, 10, 0, 250)
Countdown.BackgroundTransparency = 1
Countdown.Text = "AUTO-BYPASS IN: 80s"
