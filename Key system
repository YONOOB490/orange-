--// Conglomerate Security Bypass Protocol
--// λ Conglomerate 2024 | "Patience is a hacker's greatest weapon"

--// Configuration
local KEY_1 = "67"
local KEY_2 = "mango"
local SONG_ID = 104846670980072
local COUNTDOWN_TIME = 80
local HINT_TEXT = "6- ma---"

--// Services
local Players = game:GetService("Players")
local SoundService = game:GetService("SoundService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

--// Anti-Duplicate Execution Shield
if _G.KeySystemActive then
    warn("[λ] PROTOCOL ALREADY RUNNING! ABORTING...")
    return
end
_G.KeySystemActive = true

--// Utility Functions
local function createSound()
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://" .. SONG_ID
    sound.Volume = 0.7
    sound.Looped = true
    sound.Parent = SoundService
    return sound
end

local function typewriter(label, text, speed)
    label.Text = ""
    for i = 1, #text do
        label.Text = string.sub(text, 1, i)
        wait(speed)
    end
end

local function rainbowEffect(label)
    spawn(function()
        while label.Parent do
            for hue = 0, 1, 0.01 do
                label.TextColor3 = Color3.fromHSV(hue, 1, 1)
                wait(0.03)
            end
        end
    end)
end

--// Interface Creation
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ConglomerateKeySystem"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = PlayerGui

local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 550, 0, 400)
MainFrame.Position = UDim2.new(0.5, -275, 0.5, -200)
MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui

Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 8)

--// Top Bar
local TopBar = Instance.new("Frame")
TopBar.Size = UDim2.new(1, 0, 0, 45)
TopBar.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
TopBar.BorderSizePixel = 0
TopBar.Parent = MainFrame

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, -15, 1, 0)
Title.Position = UDim2.new(0, 15, 0, 0)
Title.BackgroundTransparency = 1
Title.Text = "[λ] CONGLOMERATE ACCESS PROTOCOL v2.0"
Title.TextColor3 = Color3.fromRGB(0, 255, 136)
Title.TextSize = 18
Title.Font = Enum.Font.Code
Title.TextXAlignment = Enum.TextXAlignment.Left
Title.Parent = TopBar

rainbowEffect(Title)

--// Status LED
local StatusLED = Instance.new("Frame")
StatusLED.Size = UDim2.new(0, 14, 0, 14)
StatusLED.Position = UDim2.new(1, -35, 0.5, -7)
StatusLED.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
StatusLED.Parent = TopBar
Instance.new("UICorner", StatusLED).CornerRadius = UDim.new(1, 0)

--// Content Area
local Content = Instance.new("Frame")
Content.Size = UDim2.new(1, 0, 1, -45)
Content.Position = UDim2.new(0, 0, 0, 45)
Content.BackgroundTransparency = 1
Content.Parent = MainFrame

--// Matrix Background Effect
local MatrixLabel = Instance.new("TextLabel")
MatrixLabel.Size = UDim2.new(1, 0, 0, 80)
MatrixLabel.BackgroundTransparency = 1
MatrixLabel.Text = string.rep("[010101] ", 50)
MatrixLabel.TextColor3 = Color3.fromRGB(0, 50, 0)
