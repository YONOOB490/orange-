-- EXPANDABLE GUI v8.2-clean (NO EXAMPLE BUTTONS)
-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°: Copy createButton ‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô comment ‡∏Ç‡∏≠‡∏á‡πÅ‡∏ó‡πá‡∏ö ‡πÅ‡∏•‡πâ‡∏ß‡πÅ‡∏Å‡πâ‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏° + ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô

local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local gui = Instance.new("ScreenGui")
gui.Name = "ExpandableGUI"
gui.ResetOnSpawn = false
gui.Parent = player.PlayerGui

-- ==================== ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏™‡∏µ ====================
local COLORS = {
    BG = Color3.fromRGB(22, 22, 32),
    HEADER = Color3.fromRGB(32, 37, 52),
    ACCENT = Color3.fromRGB(100, 140, 255),
    DANGER = Color3.fromRGB(255, 80, 80),
    WARNING = Color3.fromRGB(255, 200, 50),
    BUTTON = Color3.fromRGB(42, 47, 67),
    BUTTON_HOVER = Color3.fromRGB(57, 62, 82),
    TEXT = Color3.fromRGB(240, 240, 255)
}

-- ==================== ‡∏ä‡∏∑‡πà‡∏≠‡πÅ‡∏ó‡πá‡∏ö (‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÅ‡∏ó‡πá‡∏ö‡πÉ‡∏´‡∏°‡πà‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ ‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏™‡πà‡∏ß‡∏ô‡∏õ‡∏∏‡πà‡∏°‡∏î‡πâ‡∏≤‡∏ô‡∏•‡πà‡∏≤‡∏á) ====================
local tabNames = {
    "‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á", "‡πÄ‡∏≠‡∏°‡∏ö‡∏≠‡∏ó", "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå",
    "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô", "‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò", "‡∏≠‡∏∑‡πà‡∏ô‡πÜ"
}

-- ==================== GUI ‡∏Å‡∏£‡∏≠‡∏ö‡∏´‡∏•‡∏±‡∏Å ====================
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 340, 0, 380)
mainFrame.Position = UDim2.new(0.5, -170, 0.5, -190)
mainFrame.BackgroundColor3 = COLORS.BG
mainFrame.BackgroundTransparency = 0.2
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Parent = gui

local stroke = Instance.new("UIStroke")
stroke.Color = COLORS.ACCENT
stroke.Thickness = 2
stroke.Parent = mainFrame

-- ==================== ‡∏™‡πà‡∏ß‡∏ô‡∏´‡∏±‡∏ß ====================
local header = Instance.new("Frame")
header.Size = UDim2.new(1, 0, 0, 32)
header.BackgroundColor3 = COLORS.HEADER
header.BorderSizePixel = 0
header.Parent = mainFrame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, -80, 1, 0)
title.Position = UDim2.new(0, 10, 0, 0)
title.BackgroundTransparency = 1
title.Text = "CONTROL PANEL"
title.TextColor3 = COLORS.TEXT
title.TextSize = 12
title.Font = Enum.Font.GothamBold
title.TextXAlignment = Enum.TextXAlignment.Left
title.Parent = header

local closeBtn = Instance.new("TextButton")
closeBtn.Size = UDim2.new(0, 24, 0, 24)
closeBtn.Position = UDim2.new(1, -28, 0, 4)
closeBtn.BackgroundColor3 = COLORS.DANGER
closeBtn.BorderSizePixel = 0
closeBtn.Text = "‚úï"
closeBtn.TextColor3 = Color3.new(1,1,1)
closeBtn.TextSize = 14
closeBtn.Font = Enum.Font.GothamBold
closeBtn.Parent = header

closeBtn.MouseButton1Click:Connect(function() gui:Destroy() end)

local miniBtn = Instance.new("TextButton")
miniBtn.Size = UDim2.new(0, 24, 0, 24)
miniBtn.Position = UDim2.new(1, -56, 0, 4)
miniBtn.BackgroundColor3 = COLORS.WARNING
miniBtn.BorderSizePixel = 0
miniBtn.Text = "‚àí"
miniBtn.TextColor3 = Color3.new(1,1,1)
miniBtn.TextSize = 16
miniBtn.Font = Enum.Font.GothamBold
miniBtn.Parent = header

-- ==================== ‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏ó‡πá‡∏ö ====================
local tabWidth = 340 / #tabNames
local tabs = {}
local scrolls = {}

local tabContainer = Instance.new("Frame")
tabContainer.Size = UDim2.new(1, 0, 0, 28)
tabContainer.Position = UDim2.new(0, 0, 0, 32)
tabContainer.BackgroundColor3 = COLORS.HEADER
tabContainer.BackgroundTransparency = 0.5
tabContainer.BorderSizePixel = 0
tabContainer.Parent = mainFrame

for i, name in ipairs(tabNames) do
    tabs[name] = Instance.new("TextButton")
    tabs[name].Size = UDim2.new(0, tabWidth, 1, 0)
    tabs[name].Position = UDim2.new(0, (i-1) * tabWidth, 0, 0)
    tabs[name].BackgroundTransparency = 1
    tabs[name].Text = name
    tabs[name].TextColor3 = (i == 1) and COLORS.TEXT or Color3.fromRGB(150,150,170)
    tabs[name].TextSize = 11
    tabs[name].Font = Enum.Font.GothamBold
    tabs[name].Parent = tabContainer
    
    scrolls[name] = Instance.new("ScrollingFrame")
    scrolls[name].Size = UDim2.new(1, 0, 1, -60)
    scrolls[name].Position = UDim2.new(0, 0, 0, 60)
    scrolls[name].BackgroundTransparency = 1
    scrolls[name].BorderSizePixel = 0
    scrolls[name].ScrollBarThickness = 3
    scrolls[name].ScrollBarImageColor3 = COLORS.ACCENT
    scrolls[name].CanvasSize = UDim2.new(0, 0, 0, 0)
    scrolls[name].Visible = (i == 1)
    scrolls[name].Parent = mainFrame
    
    tabs[name].MouseButton1Click:Connect(function()
        for _, s in pairs(scrolls) do s.Visible = false end
        scrolls[name].Visible = true
        
        for _, t in pairs(tabs) do t.TextColor3 = Color3.fromRGB(150,150,170) end
        tabs[name].TextColor3 = COLORS.TEXT
    end)
end

-- ==================== ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏° ====================
local function createButton(scroll, buttonText, callback)
    local currentY = scroll.CanvasSize.Y.Offset
    
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, -10, 0, 32)
    btn.Position = UDim2.new(0, 5, 0, currentY)
    btn.BackgroundColor3 = COLORS.BUTTON
    btn.BorderSizePixel = 0
    btn.Text = buttonText
    btn.TextColor3 = COLORS.TEXT
    btn.TextSize = 10
    btn.Font = Enum.Font.GothamBold
    btn.Parent = scroll
    
    btn.MouseEnter:Connect(function() btn.BackgroundColor3 = COLORS.BUTTON_HOVER end)
    btn.MouseLeave:Connect(function() btn.BackgroundColor3 = COLORS.BUTTON end)
    btn.MouseButton1Click:Connect(callback)
    
    scroll.CanvasSize = UDim2.new(0, 0, 0, currentY + 32 + 5)
    return btn
end

-- ==================== ‡∏™‡πà‡∏ß‡∏ô‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏¢‡∏Å‡∏ï‡∏≤‡∏°‡πÅ‡∏ó‡πá‡∏ö (‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ!) ====================

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á"
-- createButton(scrolls["‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡πÄ‡∏≠‡∏°‡∏ö‡∏≠‡∏ó"
-- createButton(scrolls["‡πÄ‡∏≠‡∏°‡∏ö‡∏≠‡∏ó"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå"
-- createButton(scrolls["‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"
-- createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò"
-- createButton(scrolls["‡∏≠‡∏≤‡∏ß‡∏∏‡∏ò"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö "‡∏≠‡∏∑‡πà‡∏ô‡πÜ"
-- createButton(scrolls["‡∏≠‡∏∑‡πà‡∏ô‡πÜ"], "‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°", function() -- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡∏ô‡∏µ‡πà end)
-- SPEED CONTROL SYSTEM v2.0 - ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÉ‡∏´‡∏°‡πà
-- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ô‡∏µ‡πâ‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ‡∏Ç‡∏≠‡∏á ExpandableGUI

local SpeedSystem = {
    Enabled = false,
    Connection = nil,
    CurrentSpeed = 16,
    TargetSpeed = 16,
    DefaultSpeed = 16,
    MaxSpeed = 200, -- ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
    Smoothness = 0.1 -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡πà‡∏≤
}

local function updateSpeedLabel()
    if SpeedSystem.SpeedLabel then
        SpeedSystem.SpeedLabel.Text = "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß: " .. math.floor(SpeedSystem.CurrentSpeed)
    end
end

local function setSpeed(value)
    -- ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
    value = math.clamp(value, 0, SpeedSystem.MaxSpeed)
    SpeedSystem.TargetSpeed = value
    
    if value == SpeedSystem.DefaultSpeed then
        -- ‡∏ñ‡πâ‡∏≤‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô ‡πÉ‡∏´‡πâ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö
        if SpeedSystem.Connection then
            SpeedSystem.Connection:Disconnect()
            SpeedSystem.Connection = nil
        end
        SpeedSystem.Enabled = false
        
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÉ‡∏´‡πâ Humanoid ‡∏à‡∏£‡∏¥‡∏á
        local char = player.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum then
                hum.WalkSpeed = SpeedSystem.DefaultSpeed
                SpeedSystem.CurrentSpeed = SpeedSystem.DefaultSpeed
                updateSpeedLabel()
            end
        end
        return
    end
    
    -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏õ‡∏¥‡∏î
    if not SpeedSystem.Connection then
        SpeedSystem.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            if not SpeedSystem.Enabled then return end
            
            local char = player.Character
            if not char then return end
            local hum = char:FindFirstChildOfClass("Humanoid")
            if not hum then return end
            
            -- ‡πÉ‡∏ä‡πâ Lerp ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏•‡∏∑‡πà‡∏ô‡πÑ‡∏´‡∏•
            SpeedSystem.CurrentSpeed = SpeedSystem.CurrentSpeed + 
                (SpeedSystem.TargetSpeed - SpeedSystem.CurrentSpeed) * SpeedSystem.Smoothness
            
            -- ‡∏õ‡∏±‡∏î‡πÄ‡∏®‡∏©‡πÅ‡∏•‡∏∞‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤
            local finalSpeed = math.round(SpeedSystem.CurrentSpeed)
            if hum.WalkSpeed ~= finalSpeed then
                hum.WalkSpeed = finalSpeed
            end
            
            updateSpeedLabel()
        end)
    end
    
    SpeedSystem.Enabled = true
    updateSpeedLabel()
end

local function resetSpeed()
    setSpeed(SpeedSystem.DefaultSpeed)
    if SpeedSystem.Enabled then
        SpeedSystem.Enabled = false
    end
end

-- ============= ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏° UI =============
-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Container ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Speed Control
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "SPEED: ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î", function()
    if SpeedSystem.CurrentSpeed == SpeedSystem.DefaultSpeed then
        setSpeed(50) -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å
    else
        setSpeed(SpeedSystem.DefaultSpeed)
    end
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "SPEED: +5", function()
    setSpeed(SpeedSystem.TargetSpeed + 5)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "SPEED: +10", function()
    setSpeed(SpeedSystem.TargetSpeed + 10)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "SPEED: -5", function()
    setSpeed(SpeedSystem.TargetSpeed - 5)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "SPEED: ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï", function()
    resetSpeed()
end)

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
local speedRow = Instance.new("Frame")
speedRow.Size = UDim2.new(1, -10, 0, 28)
speedRow.BackgroundTransparency = 1
speedRow.Parent = scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]

local speedLabel = Instance.new("TextLabel")
speedLabel.Size = UDim2.new(1, 0, 1, 0)
speedLabel.BackgroundTransparency = 1
speedLabel.Text = "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß: " .. SpeedSystem.CurrentSpeed
speedLabel.TextColor3 = COLORS.TEXT
speedLabel.TextSize = 11
speedLabel.Font = Enum.Font.GothamBold
speedLabel.Parent = speedRow

SpeedSystem.SpeedLabel = speedLabel

-- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÇ‡∏´‡∏•‡∏î‡πÉ‡∏´‡∏°‡πà
speedLabel.Parent:GetPropertyChangedSignal("Visible"):Connect(function()
    if speedLabel.Parent.Visible then
        resetSpeed()
    end
end)

-- Anti-ban: ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÉ‡∏Å‡∏•‡πâ‡πÅ‡∏≠‡∏î‡∏°‡∏¥‡∏ô
game:GetService("Players").PlayerAdded:Connect(function(p)
    if p:GetRankInGroup(0) > 0 or p.Name:find("Admin") then
        SpeedSystem.MaxSpeed = 50
        if SpeedSystem.TargetSpeed > 50 then
            setSpeed(50)
        end
    end
end)

print("[Speed] Speed Control v2.0 Loaded - ‡∏ß‡∏≤‡∏á‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢")
-- JUMP POWER SYSTEM v2.0 - ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏Å‡∏≤‡∏£‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ + ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ä‡πà‡∏ß‡∏á‡∏õ‡∏£‡∏±‡∏ö 10-300
-- ‡∏≠‡∏≠‡∏Å‡πÅ‡∏ö‡∏ö‡πÉ‡∏´‡πâ‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Å‡∏±‡∏ö GUI ‡πÉ‡∏´‡∏°‡πà‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á

local JumpSystem = {
    Enabled = false,
    Connection = nil,
    CurrentPower = 50,
    DefaultPower = 50,
    MinPower = 10,
    MaxPower = 300
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å: ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ JumpPower ‡πÅ‡∏ö‡∏ö‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
local function setJumpPower(value)
    -- ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏Ñ‡πà‡∏≤‡πÉ‡∏ô‡∏ä‡πà‡∏ß‡∏á‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î
    value = math.clamp(value, JumpSystem.MinPower, JumpSystem.MaxPower)
    JumpSystem.CurrentPower = value
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÉ‡∏´‡πâ‡∏Å‡∏±‡∏ö Humanoid ‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà
    local char = player.Character
    if char then
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            -- ‡∏ï‡∏±‡πâ‡∏á‡∏ó‡∏±‡πâ‡∏á JumpPower ‡πÅ‡∏•‡∏∞ JumpHeight (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏ö‡∏≤‡∏á‡πÄ‡∏Å‡∏°)
            hum.JumpPower = value
            hum.JumpHeight = value * 0.6
            
            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ã‡πâ‡∏≥‡∏ß‡πà‡∏≤‡∏Ñ‡πà‡∏≤‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á
            if hum.JumpPower ~= value then
                hum.JumpPower = value -- ‡∏ï‡∏±‡πâ‡∏á‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ñ‡πâ‡∏≤‡πÄ‡∏Å‡∏°‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç
            end
        end
    end
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó UI
    if JumpSystem.StatusLabel then
        JumpSystem.StatusLabel.Text = "‡πÅ‡∏£‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î: " .. value
    end
    
    return value
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏´‡∏•‡∏±‡∏Å)
local function resetJumpPower()
    -- ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏Å‡πà‡∏≠‡∏ô
    if JumpSystem.Connection then
        JumpSystem.Connection:Disconnect()
        JumpSystem.Connection = nil
    end
    JumpSystem.Enabled = false
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    setJumpPower(JumpSystem.DefaultPower)
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÉ‡∏´‡πâ Humanoid ‡∏ó‡∏∏‡∏Å‡∏ï‡∏±‡∏ß‡πÉ‡∏ô‡πÄ‡∏Å‡∏°
    for _, p in ipairs(game.Players:GetPlayers()) do
        if p.Character then
            local hum = p.Character:FindFirstChildOfClass("Humanoid")
            if hum then
                hum.JumpPower = JumpSystem.DefaultPower
                hum.JumpHeight = JumpSystem.DefaultPower * 0.6
            end
        end
    end
    
    print("[Jump] ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÅ‡∏•‡πâ‡∏ß:", JumpSystem.DefaultPower)
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£ respawn ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
local function onCharacterAdded(char)
    task.wait(0.3) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏û‡∏£‡πâ‡∏≠‡∏°
    local hum = char:WaitForChild("Humanoid", 5)
    if hum then
        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡πÄ‡∏Ñ‡∏¢‡∏ï‡∏±‡πâ‡∏á‡πÑ‡∏ß‡πâ‡πÉ‡∏´‡∏°‡πà
        hum.JumpPower = JumpSystem.CurrentPower
        hum.JumpHeight = JumpSystem.CurrentPower * 0.6
    end
end

if player.Character then
    onCharacterAdded(player.Character)
end
player.CharacterAdded:Connect(onCharacterAdded)

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
local function toggleAutoSystem()
    JumpSystem.Enabled = not JumpSystem.Enabled
    
    if JumpSystem.Enabled then
        JumpSystem.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            local char = player.Character
            if char then
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum and hum.JumpPower ~= JumpSystem.CurrentPower then
                    hum.JumpPower = JumpSystem.CurrentPower
                    hum.JumpHeight = JumpSystem.CurrentPower * 0.6
                end
            end
        end)
        print("[Jump] ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏õ‡∏¥‡∏î")
    else
        if JumpSystem.Connection then
            JumpSystem.Connection:Disconnect()
            JumpSystem.Connection = nil
        end
        print("[Jump] ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏õ‡∏¥‡∏î")
    end
end

-- ============== ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ó‡πá‡∏ö "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ==============

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏•‡∏±‡∏Å
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î: ‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥", function()
    toggleAutoSystem()
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏û‡∏¥‡πà‡∏°/‡∏•‡∏î‡∏Ñ‡πà‡∏≤
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "+10 ‡πÅ‡∏£‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î", function()
    setJumpPower(JumpSystem.CurrentPower + 10)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "-10 ‡πÅ‡∏£‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î", function()
    setJumpPower(JumpSystem.CurrentPower - 10)
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏û‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÄ‡∏ö‡∏≤ (20)", function()
    setJumpPower(20)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏õ‡∏Å‡∏ï‡∏¥ (50)", function()
    setJumpPower(50)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á (100)", function()
    setJumpPower(100)
end)

createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏∏‡∏î (300)", function()
    setJumpPower(300)
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÅ‡∏ö‡∏ö Hard Reset (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ)
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô (‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ)", function()
    resetJumpPower()
end)

-- Label ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local statusFrame = Instance.new("Frame")
statusFrame.Size = UDim2.new(1, -10, 0, 25)
statusFrame.BackgroundTransparency = 1
statusFrame.Parent = scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]

local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, 0, 1, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô (‡∏Ñ‡πà‡∏≤‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô: " .. JumpSystem.CurrentPower .. ")"
statusLabel.TextColor3 = COLORS.TEXT
statusLabel.TextSize = 10
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = statusFrame

JumpSystem.StatusLabel = statusLabel

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ó‡∏∏‡∏Å‡πÜ 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
game:GetService("RunService").Heartbeat:Connect(function()
    if JumpSystem.Enabled then
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ ON (‡∏Ñ‡πà‡∏≤: " .. JumpSystem.CurrentPower .. ")"
    else
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ OFF (‡∏Ñ‡πà‡∏≤: " .. JumpSystem.CurrentPower .. ")"
    end
end)

-- Cleanup
gui.Destroying:Connect(function()
    if JumpSystem.Connection then JumpSystem.Connection:Disconnect() end
    resetJumpPower()
end)

print("[Jump] Jump Power v2.0 ‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à - ‡πÅ‡∏Å‡πâ‡∏ö‡∏±‡∏Ñ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï + ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÑ‡∏î‡πâ‡πÄ‡∏ï‡πá‡∏°‡∏ó‡∏µ‡πà")
-- INFINITE JUMP v2.0 - ‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÉ‡∏´‡∏°‡πà
-- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ô‡∏µ‡πâ‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ‡∏Ç‡∏≠‡∏á ExpandableGUI

local InfiniteJump = {
    Enabled = false,
    Connection = nil,
    Settings = {
        HighJumpMode = false, -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏û‡∏¥‡πÄ‡∏®‡∏©
        AutoRejump = true -- ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏Å
    }
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å: ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡πÉ‡∏´‡πâ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
local function forceJump()
    local char = player.Character
    if not char then return end
    
    local hum = char:FindFirstChildOfClass("Humanoid")
    local hrp = char:FindFirstChild("HumanoidRootPart")
    
    if hum and hrp then
        -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÄ‡∏õ‡πá‡∏ô Jumping
        hum:ChangeState(Enum.HumanoidStateType.Jumping)
        
        -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡πá‡∏ô HighJumpMode
        if InfiniteJump.Settings.HighJumpMode then
            hum.JumpPower = 120
            hrp.Velocity = Vector3.new(0, 80, 0)
        else
            hum.JumpPower = 50
            hrp.Velocity = Vector3.new(0, hrp.Velocity.Y, 0)
        end
        
        -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô
        task.wait(0.05)
        if hum:GetState() == Enum.HumanoidStateType.Landed then
            hum:ChangeState(Enum.HumanoidStateType.Freefall)
        end
    end
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Å‡∏î Jump ‡∏ö‡∏ô‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠/PC
local function setupJumpListener()
    local UserInputService = game:GetService("UserInputService")
    
    -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö PC: ‡πÉ‡∏ä‡πâ JumpRequest
    InfiniteJump.Connection = UserInputService.JumpRequest:Connect(function()
        if not InfiniteJump.Enabled then return end
        
        local char = player.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum and hum:GetState() ~= Enum.HumanoidStateType.Jumping then
                forceJump()
            end
        end
    end)
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏ï‡∏Å‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
local function setupFallDetector()
    game:GetService("RunService").Heartbeat:Connect(function()
        if not InfiniteJump.Enabled then return end
        if not InfiniteJump.Settings.AutoRejump then return end
        
        local char = player.Character
        if char then
            local hrp = char:FindFirstChild("HumanoidRootPart")
            if hrp and hrp.Velocity.Y < -50 then -- ‡∏ï‡∏Å‡πÄ‡∏£‡πá‡∏ß
                task.wait(0.1)
                if hrp.Velocity.Y < -50 then
                    forceJump()
                end
            end
        end
    end)
end

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö
local function toggleInfiniteJump()
    InfiniteJump.Enabled = not InfiniteJump.Enabled
    
    if InfiniteJump.Enabled then
        setupJumpListener()
        print("[InfiniteJump] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
    else
        if InfiniteJump.Connection then
            InfiniteJump.Connection:Disconnect()
            InfiniteJump.Connection = nil
        end
        print("[InfiniteJump] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
    end
    
    if InfiniteJump.StatusLabel then
        InfiniteJump.StatusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: " .. (InfiniteJump.Enabled and "‡πÄ‡∏õ‡∏¥‡∏î" or "‡∏õ‡∏¥‡∏î")
    end
end

-- ============== ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ó‡πá‡∏ö "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ==============

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏´‡∏•‡∏±‡∏Å
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î: ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î", function()
    toggleInfiniteJump()
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÇ‡∏´‡∏°‡∏î
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á: ‡∏õ‡∏¥‡∏î", function()
    InfiniteJump.Settings.HighJumpMode = not InfiniteJump.Settings.HighJumpMode
    local btn = scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]:FindFirstChild("‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á: ‡∏õ‡∏¥‡∏î") or scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]:FindFirstChild("‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á: ‡πÄ‡∏õ‡∏¥‡∏î")
    if btn then
        btn.Text = "‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á: " .. (InfiniteJump.Settings.HighJumpMode and "‡πÄ‡∏õ‡∏¥‡∏î" or "‡∏õ‡∏¥‡∏î")
        btn.BackgroundColor3 = InfiniteJump.Settings.HighJumpMode and COLORS.WARNING or COLORS.BUTTON
    end
end)

-- Label ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local statusRow = Instance.new("Frame")
statusRow.Size = UDim2.new(1, -10, 0, 25)
statusRow.BackgroundTransparency = 1
statusRow.Parent = scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]

local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, 0, 1, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏õ‡∏¥‡∏î"
statusLabel.TextColor3 = COLORS.TEXT
statusLabel.TextSize = 10
statusLabel.Font = Enum.Font.GothamBold
statusLabel.Parent = statusRow

InfiniteJump.StatusLabel = statusLabel

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
setupFallDetector()

-- Cleanup
gui.Destroying:Connect(function()
    if InfiniteJump.Connection then
        InfiniteJump.Connection:Disconnect()
    end
end)

print("[InfiniteJump] Infinite Jump v2.0 Loaded - ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÉ‡∏ô‡∏≠‡∏≤‡∏Å‡∏≤‡∏®‡πÑ‡∏î‡πâ‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î")
-- NOCLIP v2.0 - ‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏™‡∏¥‡πà‡∏á‡∏Å‡∏µ‡∏î‡∏Ç‡∏ß‡∏≤‡∏á (‡∏™‡∏±‡πà‡∏á‡∏á‡∏≤‡∏ô‡∏á‡πà‡∏≤‡∏¢ ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ö‡∏±‡∏Ñ)
-- ‡∏ß‡∏≤‡∏á‡πÇ‡∏Ñ‡πâ‡∏î‡∏ô‡∏µ‡πâ‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ‡∏Ç‡∏≠‡∏á ExpandableGUI

local NoclipSystem = {
    Enabled = false,
    Connection = nil,
    Settings = {
        RevertOnDeath = true, -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏≤‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
        UseStepped = true -- ‡πÉ‡∏ä‡πâ Stepped ‡πÅ‡∏ó‡∏ô Heartbeat ‡∏•‡∏î‡∏Å‡∏≤‡∏£‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å
    }
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å: ‡∏ï‡∏±‡πâ‡∏á CanCollide ‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function setNoclipState(enabled)
    local char = player.Character
    if not char then return end
    
    for _, part in ipairs(char:GetDescendants()) do
        if part:IsA("BasePart") then
            -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î Noclip ‡πÉ‡∏´‡πâ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô ‡∏ñ‡πâ‡∏≤‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô‡∏Å‡∏•‡∏±‡∏ö
            part.CanCollide = not enabled
        end
    end
    
    -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏û‡∏¥‡πÄ‡∏®‡∏©‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö HumanoidRootPart (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏ï‡∏¥‡∏î)
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.CanCollide = false
    end
end

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö Noclip
local function toggleNoclip()
    NoclipSystem.Enabled = not NoclipSystem.Enabled
    
    if NoclipSystem.Enabled then
        -- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏Å‡∏±‡∏ö Stepped (‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Physics)
        local service = NoclipSystem.Settings.UseStepped and game:GetService("RunService").Stepped or game:GetService("RunService").Heartbeat
        
        NoclipSystem.Connection = service:Connect(function()
            if not NoclipSystem.Enabled then return end
            setNoclipState(true)
        end)
        
        -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å
        setNoclipState(true)
        print("[Noclip] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
    else
        -- ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠
        if NoclipSystem.Connection then
            NoclipSystem.Connection:Disconnect()
            NoclipSystem.Connection = nil
        end
        
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏Å‡∏≤‡∏£‡∏ä‡∏±‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÄ‡∏õ‡πá‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥
        setNoclipState(false)
        print("[Noclip] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
    end
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó UI
    if NoclipSystem.StatusLabel then
        NoclipSystem.StatusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: " .. (NoclipSystem.Enabled and "‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏" or "‡∏õ‡∏Å‡∏ï‡∏¥")
    end
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£ (Respawn)
local function onCharacterAdded(char)
    task.wait(0.2) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏û‡∏£‡πâ‡∏≠‡∏°
    if NoclipSystem.Enabled then
        setNoclipState(true)
    end
end

-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏Å‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
if player.Character then
    onCharacterAdded(player.Character)
end
player.CharacterAdded:Connect(onCharacterAdded)

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏ï‡∏≤‡∏¢ (‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î RevertOnDeath)
if NoclipSystem.Settings.RevertOnDeath then
    player.Character:WaitForChild("Humanoid").Died:Connect(function()
        if NoclipSystem.Enabled then
            -- ‡∏õ‡∏¥‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡∏´‡∏•‡∏±‡∏á‡∏ï‡∏≤‡∏¢
            toggleNoclip()
            task.wait(1.5)
            toggleNoclip()
        end
    end)
end

-- ============== ‡∏™‡∏£‡πâ‡∏≤‡∏á UI ‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö "‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" ==============

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏´‡∏•‡∏±‡∏Å
createButton(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"], "‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏ (Noclip): ‡∏õ‡∏¥‡∏î", function()
    toggleNoclip()
    
    -- ‡∏´‡∏≤‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡∏û‡∏∂‡πà‡∏á‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
    local btnName = "‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏ (Noclip): " .. (NoclipSystem.Enabled and "‡πÄ‡∏õ‡∏¥‡∏î" or "‡∏õ‡∏¥‡∏î")
    local targetBtn = nil
    
    for _, child in ipairs(scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]:GetChildren()) do
        if child:IsA("TextButton") and child.Text:find("‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏") then
            targetBtn = child
            break
        end
    end
    
    if targetBtn then
        targetBtn.Text = btnName
        targetBtn.BackgroundColor3 = NoclipSystem.Enabled and COLORS.WARNING or COLORS.BUTTON
    end
end)

-- Label ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local statusRow = Instance.new("Frame")
statusRow.Size = UDim2.new(1, -10, 0, 25)
statusRow.BackgroundTransparency = 1
statusRow.Parent = scrolls["‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"]

local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, 0, 1, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏õ‡∏Å‡∏ï‡∏¥ (‡πÄ‡∏î‡∏¥‡∏ô‡∏ä‡∏ô‡∏™‡∏¥‡πà‡∏á‡∏Å‡∏µ‡∏î‡∏Ç‡∏ß‡∏≤‡∏á‡πÑ‡∏î‡πâ)"
statusLabel.TextColor3 = COLORS.TEXT
statusLabel.TextSize = 10
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = statusRow

NoclipSystem.StatusLabel = statusLabel

-- Cleanup ‡πÄ‡∏°‡∏∑‡πà‡∏≠ GUI ‡∏õ‡∏¥‡∏î
gui.Destroying:Connect(function()
    if NoclipSystem.Connection then
        NoclipSystem.Connection:Disconnect()
    end
    setNoclipState(false) -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏Å‡πà‡∏≠‡∏ô‡∏õ‡∏¥‡∏î‡πÄ‡∏™‡∏°‡∏≠
end)

print("[Noclip] Noclip v2.0 Loaded - ‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏™‡∏¥‡πà‡∏á‡∏Å‡∏µ‡∏î‡∏Ç‡∏ß‡∏≤‡∏á‡πÑ‡∏î‡πâ")

-- ==================== ‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏¢‡πà‡∏≠ ====================
local miniFrame = Instance.new("Frame")
miniFrame.Size = UDim2.new(0, 40, 0, 40)
miniFrame.Position = UDim2.new(1, -50, 1, -50)
miniFrame.BackgroundColor3 = COLORS.HEADER
miniFrame.BackgroundTransparency = 0.2
miniFrame.BorderSizePixel = 0
miniFrame.Visible = false
miniFrame.Parent = gui

local miniStroke = Instance.new("UIStroke")
miniStroke.Color = COLORS.ACCENT
miniStroke.Thickness = 2
miniStroke.Parent = miniFrame

local restoreBtn = Instance.new("TextButton")
restoreBtn.Size = UDim2.new(1, 0, 1, 0)
restoreBtn.BackgroundTransparency = 1
restoreBtn.Text = "‚òê"
restoreBtn.TextColor3 = Color3.new(1,1,1)
restoreBtn.TextSize = 16
restoreBtn.Font = Enum.Font.GothamBold
restoreBtn.Parent = miniFrame

miniBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
    miniFrame.Visible = true
end)

restoreBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    miniFrame.Visible = false
end)

-- ==================== ‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡∏≤‡∏Å‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠ ====================
local dragging = false
local dragStartPos = Vector2.zero
local frameStartPos = UDim2.zero
local screenSize = workspace.CurrentCamera.ViewportSize

header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStartPos = Vector2.new(mouse.X, mouse.Y)
        frameStartPos = mainFrame.Position
    end
end)

mouse.Move:Connect(function()
    if dragging then
        local delta = Vector2.new(mouse.X, mouse.Y) - dragStartPos
        local newX = frameStartPos.X.Offset + delta.X
        local newY = frameStartPos.Y.Offset + delta.Y
        
        newX = math.clamp(newX, 0, screenSize.X - mainFrame.AbsoluteSize.X)
        newY = math.clamp(newY, 0, screenSize.Y - mainFrame.AbsoluteSize.Y)
        
        mainFrame.Position = UDim2.new(0, newX, 0, newY)
    end
end)

mouse.Button1Up:Connect(function() 
    dragging = false 
end)

workspace.CurrentCamera:GetPropertyChangedSignal("ViewportSize"):Connect(function()
    screenSize = workspace.CurrentCamera.ViewportSize
end)

-- ==================== ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô ====================
print("‚úÖ GUI v8.2-clean Loaded! ‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡πÅ‡∏ó‡πá‡∏ö: " .. #tabNames)
print("üìå ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°: Copy createButton ‡πÉ‡∏ô‡∏™‡πà‡∏ß‡∏ô‡πÅ‡∏ó‡πá‡∏ö‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ ‡πÅ‡∏•‡πâ‡∏ß‡πÅ‡∏ó‡∏ô‡∏ó‡∏µ‡πà‡∏î‡πâ‡∏ß‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏° + ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì")
