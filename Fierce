        v.BackgroundColor3 = AccentColor
        v.BorderSizePixel = 0
        v.Parent = CrosshairObj
        
        ShowNotif("‚úÖ Crosshair enabled", 2)
    else
        if CrosshairObj then
            CrosshairObj:Destroy()
        end
        ShowNotif("‚ùå Crosshair disabled", 2)
    end
end)
AddHover(CrosshairToggle)

