-- Tab Switching Logic (‡∏£‡∏ß‡∏°‡∏Å‡∏±‡∏ö Part 1 & 2)
for i, btn in ipairs(TabButtons) do
    btn.MouseButton1Click:Connect(function()
        if CurrentTab == i then return end
        TabContents[CurrentTab].Visible = false
        CurrentTab = i
        TabContents[i].Visible = true
        for j, button in ipairs(TabButtons) do
            TweenService:Create(button, TweenInfoSmooth, {TextColor3 = (j == i and AccentColor or Color3.new(1,1,1))}):Play()
        end
    end)
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn.UIStroke, TweenInfoSmooth, {Transparency = 0.4}):Play()
    end)
    
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn.UIStroke, TweenInfoSmooth, {Transparency = 0.9}):Play()
    end)
end

-- Initialize Game Detection
local CurrentGame = "Unknown"
spawn(function()
    local placeId = game.PlaceId
    local supportedGames = {
        [2753915549] = "Blox Fruits", [4442272183] = "Blox Fruits", [7449423635] = "Blox Fruits",
        [6284583030] = "Pet Simulator X", [10321372166] = "Pet Simulator X",
        [286090429] = "Arsenal"
    }
    CurrentGame = supportedGames[placeId] or "Unknown"
    if CurrentGame ~= "Unknown" then
        ShowNotif("üéÆ " .. CurrentGame .. " detected!", 3)
    end
end)

