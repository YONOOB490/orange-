-- Enhanced Hover for ESP Buttons
local function AddHoverEnhanced(btn)
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfoSmooth, {BackgroundTransparency = 0.25}):Play()
        TweenService:Create(btn, TweenInfoSmooth, {TextColor3 = AccentColor}):Play()
        TweenService:Create(btn.UIStroke, TweenInfoSmooth, {Transparency = 0.3}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfoSmooth, {BackgroundTransparency = 0.6}):Play()
        TweenService:Create(btn, TweenInfoSmooth, {TextColor3 = Color3.new(1,1,1)}):Play()
        TweenService:Create(btn.UIStroke, TweenInfoSmooth, {Transparency = 0.6}):Play()
    end)
end

AddHoverEnhanced(ESPToggle)
AddHoverEnhanced(HighlightToggle)

