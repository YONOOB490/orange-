    FullbrightToggle.Text = FullbrightEnabled and "FULLBRIGHT: ON" or "FULLBRIGHT: OFF"
    TweenService:Create(FullbrightToggle, TweenInfoSmooth, {BackgroundTransparency = FullbrightEnabled and 0.3 or 0.6}):Play()
    
    if FullbrightEnabled then
        Lighting.Brightness = 2
        Lighting.GlobalShadows = false
        Lighting.Ambient = Color3.fromRGB(255, 255, 255)
        Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
        ShowNotif("‚úÖ Fullbright enabled", 2)
    else
        Lighting.Brightness = 1
        Lighting.GlobalShadows = true
        Lighting.Ambient = Color3.fromRGB(127, 127, 127)
        Lighting.OutdoorAmbient = Color3.fromRGB(127, 127, 127)
        ShowNotif("‚ùå Fullbright disabled", 2)
    end
end)
AddHover(FullbrightToggle)

