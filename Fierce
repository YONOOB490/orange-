--============== Fierce Executor v1.5.3 (Part 1/3 - Core) ===============
-- ‡∏Ñ‡∏≥‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô: ‡πÉ‡∏ä‡πâ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Å‡∏≤‡∏£‡∏®‡∏∂‡∏Å‡∏©‡∏≤‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô ‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏≠‡∏≤‡∏à‡∏ù‡πà‡∏≤‡∏ù‡∏∑‡∏ô ToS ‡∏Ç‡∏≠‡∏á Roblox
-- ‡∏ú‡∏π‡πâ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÑ‡∏°‡πà‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∞‡∏Å‡∏±‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
-- ‡∏û‡∏≤‡∏£‡πå‡∏ó 1: ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô + GUI Framework
--=========================================================================

local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local HttpService = game:GetService("HttpService")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "FierceExecutor"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = PlayerGui

-- Colors & Themes
local DarkBase = Color3.fromRGB(30, 10, 0)
local MainColor = Color3.fromRGB(255, 90, 20)
local DarkColor = Color3.fromRGB(200, 50, 0)
local AccentColor = Color3.fromRGB(255, 140, 50)
local Transparent = 0.12

-- Tween Configs
local TweenInfoSmooth = TweenInfo.new(0.35, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
local TweenInfoBounce = TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out)

-- GUI Sizes
local NormalSize = UDim2.new(0, 600, 0, 480)
local LargeSize = UDim2.new(0, 900, 0, 650)
local NormalPos = UDim2.new(0.5, -300, 0.5, -240)
local LargePos = UDim2.new(0.5, -450, 0.5, -325)

-- Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = NormalSize
MainFrame.Position = NormalPos
MainFrame.BackgroundColor3 = DarkBase
MainFrame.BackgroundTransparency = Transparent
MainFrame.BorderSizePixel = 0
MainFrame.ClipsDescendants = true
MainFrame.Visible = true
MainFrame.Parent = ScreenGui

local MainGradient = Instance.new("UIGradient")
MainGradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, DarkBase), ColorSequenceKeypoint.new(1, MainColor)})
MainGradient.Rotation = 30
MainGradient.Parent = MainFrame

local MainCorner = Instance.new("UICorner")
MainCorner.CornerRadius = UDim.new(0, 14)
MainCorner.Parent = MainFrame

local MainGlow = Instance.new("UIStroke")
MainGlow.Transparency = 0.6
MainGlow.Color = AccentColor
MainGlow.Thickness = 2
MainGlow.Parent = MainFrame

-- Notification System
local NotifY = 20
local function ShowNotif(text, duration)
    duration = duration or 3
    local notif = Instance.new("TextLabel")
    notif.Size = UDim2.new(0, 300, 0, 50)
    notif.Position = UDim2.new(1, 320, 0, NotifY)
    notif.BackgroundColor3 = DarkBase
    notif.BackgroundTransparency = 0.2
    notif.BorderSizePixel = 0
    notif.Text = " " .. text
    notif.TextColor3 = AccentColor
    notif.TextSize = 16
    notif.Font = Enum.Font.GothamBold
    notif.TextXAlignment = Enum.TextXAlignment.Left
    notif.ZIndex = 1000
    notif.Parent = ScreenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = notif
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = AccentColor
    stroke.Thickness = 2
    stroke.Transparency = 0.6
    stroke.Parent = notif
    
    local icon = Instance.new("TextLabel")
    icon.Size = UDim2.new(0, 30, 0, 30)
    icon.Position = UDim2.new(0, 10, 0.5, -15)
    icon.BackgroundTransparency = 1
    icon.Text = "üíÄ"
    icon.TextScaled = true
    icon.TextColor3 = AccentColor
    icon.Font = Enum.Font.GothamBlack
    icon.Parent = notif
    
    NotifY = NotifY + 60
    TweenService:Create(notif, TweenInfoSmooth, {Position = UDim2.new(1, -270, 0, notif.Position.Y.Offset)}):Play()
    
    spawn(function()
        wait(duration)
        TweenService:Create(notif, TweenInfoSmooth, {Position = UDim2.new(1, 320, 0, notif.Position.Y.Offset)}):Play()
        wait(0.4)
        notif:Destroy()
        NotifY = NotifY - 60
    end)
end

-- Global Variables
local WalkSpeedValue = 50
local JumpPowerValue = 50
local NoclipEnabled = false
local NoclipConnection = nil
local ESPHighlightSettings = {Enabled = false}
local ESPNameSettings = {Enabled = false, ShowName = true, ShowDistance = true, ShowHealth = true, TextSize = 10, Range = 500}
local ESPConnection = nil
local ESPObjects = {}

-- Utility Functions
local function GetCharacter()
    local char = Player.Character
    if not char or not char.Parent then
        char = Player.CharacterAdded:Wait()
    end
    return char
end

local function GetHumanoid()
    local char = GetCharacter()
    return char and char:FindFirstChildOfClass("Humanoid")
end

local function ApplyMovementMods()
    local hum = GetHumanoid()
    if hum then
        hum.WalkSpeed = WalkSpeedValue
        hum.JumpPower = JumpPowerValue
        ShowNotif("‚úÖ Movement mods applied", 2)
    else
        ShowNotif("‚ùå Humanoid not found", 2)
    end
end

local function SaveConfig()
    pcall(function()
        local config = {ws = WalkSpeedValue, jp = JumpPowerValue}
        if writefile then
            writefile("fierce_config.json", HttpService:JSONEncode(config))
        end
    end)
end

local function LoadConfig()
    pcall(function()
        if readfile and isfile and isfile("fierce_config.json") then
            local data = readfile("fierce_config.json")
            local config = HttpService:JSONDecode(data)
            WalkSpeedValue = config.ws or 50
            JumpPowerValue = config.jp or 50
            ShowNotif("‚úÖ Config loaded", 2)
        end
    end)
end

LoadConfig()

-- ========== ESP SYSTEMS (‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ) ==========
local ESPHighlightSettings = {Enabled = false}
local ESPNameSettings = {Enabled = false, ShowName = true, ShowDistance = true, ShowHealth = true, TextSize = 10, Range = 500}
local ESPConnection = nil
local ESPObjects = {}

local function CreateESPName(targetPlayer)
    if ESPObjects[targetPlayer] then return end
    
    local billboard = Instance.new("BillboardGui")
    billboard.Size = UDim2.new(0, 120, 0, 35)
    billboard.StudsOffset = Vector3.new(0, 2.5, 0)
    billboard.AlwaysOnTop = true
    billboard.Parent = ScreenGui
    
    local bg = Instance.new("Frame")
    bg.Size = UDim2.new(1, 0, 1, 0)
    bg.BackgroundColor3 = Color3.fromRGB(0,0,0)
    bg.BackgroundTransparency = 0.6
    bg.BorderSizePixel = 0
    bg.Parent = billboard
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 4)
    corner.Parent = bg
    
    -- Name Label
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -10, 0, 12)
    nameLabel.Position = UDim2.new(0, 5, 0, 2)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = targetPlayer.Name
    nameLabel.TextColor3 = AccentColor
    nameLabel.TextSize = ESPNameSettings.TextSize
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.Parent = bg
    
    -- Distance Label
    local distLabel = Instance.new("TextLabel")
    distLabel.Size = UDim2.new(1, -10, 0, 10)
    distLabel.Position = UDim2.new(0, 5, 0, 14)
    distLabel.BackgroundTransparency = 1
    distLabel.Text = ""
    distLabel.TextColor3 = Color3.new(0.9,0.9,0.9)
    distLabel.TextSize = ESPNameSettings.TextSize - 1
    distLabel.Font = Enum.Font.Gotham
    distLabel.Parent = bg
    
    -- Health Label
    local healthLabel = Instance.new("TextLabel")
    healthLabel.Size = UDim2.new(1, -10, 0, 10)
    healthLabel.Position = UDim2.new(0, 5, 0, 23)
    healthLabel.BackgroundTransparency = 1
    healthLabel.Text = ""
    healthLabel.TextColor3 = Color3.new(0,1,0)
    healthLabel.TextSize = ESPNameSettings.TextSize - 1
    healthLabel.Font = Enum.Font.Gotham
    healthLabel.Parent = bg
    
    ESPObjects[targetPlayer] = {billboard = billboard}
    
    -- Update Loop
    spawn(function()
        while ESPNameSettings.Enabled and Players:FindFirstChild(targetPlayer.Name) do
            local char = targetPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local myChar = GetCharacter()
                if myChar and myChar:FindFirstChild("HumanoidRootPart") then
                    local distance = (char.HumanoidRootPart.Position - myChar.HumanoidRootPart.Position).Magnitude
                    if distance <= ESPNameSettings.Range then
                        billboard.Enabled = true
                        billboard.Adornee = char.HumanoidRootPart
                        distLabel.Text = string.format("%.0fm", distance)
                        local humanoid = char:FindFirstChildOfClass("Humanoid")
                        if humanoid then
                            healthLabel.Text = string.format("%d/%d", humanoid.Health, humanoid.MaxHealth)
                        end
                    else
                        billboard.Enabled = false
                    end
                end
            else
                billboard.Enabled = false
            end
            wait(0.15)
        end
        billboard:Destroy()
        ESPObjects[targetPlayer] = nil
    end)
end

local function ToggleESPName()
    ESPNameSettings.Enabled = not ESPNameSettings.Enabled
    
    if ESPNameSettings.Enabled then
        -- Create for existing players
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= Player then
                CreateESPName(plr)
            end
        end
        
        -- Listen for new players
        ESPConnection = Players.PlayerAdded:Connect(function(newPlayer)
            if ESPNameSettings.Enabled then
                wait(1)
                CreateESPName(newPlayer)
            end
        end)
        
        ShowNotif("‚úÖ ESP Name enabled", 2)
    else
        -- Disconnect listener
        if ESPConnection then
            ESPConnection:Disconnect()
            ESPConnection = nil
        end
        
        -- Destroy all ESP objects
        for _, obj in pairs(ESPObjects) do
            if obj.billboard then
                obj.billboard:Destroy()
            end
        end
        ESPObjects = {}
        ShowNotif("‚ùå ESP Name disabled", 2)
    end
end

local function ToggleESPHighlight()
    ESPHighlightSettings.Enabled = not ESPHighlightSettings.Enabled
    local highlightObjects = {}
    
    if ESPHighlightSettings.Enabled then
        -- Create for existing players
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= Player then
                local highlight = Instance.new("Highlight")
                highlight.FillColor = AccentColor
                highlight.OutlineColor = Color3.new(1,1,1)
                highlight.OutlineTransparency = 0.5
                highlight.FillTransparency = 0.7
                highlight.Parent = ScreenGui
                highlightObjects[plr] = highlight
                
                spawn(function()
                    while ESPHighlightSettings.Enabled and Players:FindFirstChild(plr.Name) do
                        local char = plr.Character
                        if char then
                            highlight.Adornee = char
                        end
                        wait(1)
                    end
                    highlight:Destroy()
                    highlightObjects[plr] = nil
                end)
            end
        end
        
        -- Listen for new players
        ESPConnection = Players.PlayerAdded:Connect(function(newPlayer)
            if ESPHighlightSettings.Enabled and newPlayer ~= Player then
                wait(1)
                local highlight = Instance.new("Highlight")
                highlight.FillColor = AccentColor
                highlight.OutlineColor = Color3.new(1,1,1)
                highlight.OutlineTransparency = 0.5
                highlight.FillTransparency = 0.7
                highlight.Parent = ScreenGui
                
                spawn(function()
                    while ESPHighlightSettings.Enabled and Players:FindFirstChild(newPlayer.Name) do
                        local char = newPlayer.Character
                        if char then
                            highlight.Adornee = char
                        end
                        wait(1)
                    end
                    highlight:Destroy()
                end)
            end
        end)
        
        ShowNotif("‚úÖ ESP Highlight enabled", 2)
    else
        -- Disconnect listener
        if ESPConnection then
            ESPConnection:Disconnect()
            ESPConnection = nil
        end
        
        -- Destroy all highlights
        for _, obj in pairs(highlightObjects) do
            if obj then
                obj:Destroy()
            end
        end
        ShowNotif("‚ùå ESP Highlight disabled", 2)
    end
end

