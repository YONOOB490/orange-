local function CreateScriptButton(data, index)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, -10, 0, 70)
    btn.Position = UDim2.new(0, 5, 0, (index-1)*75)
    btn.BackgroundColor3 = DarkBase
    btn.BackgroundTransparency = 0.5
    btn.Text = ""
    btn.Parent = HubScroll
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = btn
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = AccentColor
    stroke.Thickness = 1
    stroke.Transparency = 0.8
    stroke.Parent = btn
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -20, 0.4, 0)
    title.Position = UDim2.new(0, 10, 0, 5)
    title.BackgroundTransparency = 1
    title.Text = data.name
    title.TextColor3 = Color3.new(1,1,1)
    title.TextSize = 18
    title.Font = Enum.Font.GothamBold
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = btn
    
    local desc = Instance.new("TextLabel")
    desc.Size = UDim2.new(1, -20, 0.3, 0)
    desc.Position = UDim2.new(0, 10, 0.4, 0)
    desc.BackgroundTransparency = 1
    desc.Text = data.desc
    desc.TextColor3 = Color3.new(0.8,0.8,0.8)
    desc.TextSize = 12
    desc.Font = Enum.Font.Gotham
    desc.TextXAlignment = Enum.TextXAlignment.Left
    desc.Parent = btn
    
    local keyStatus = Instance.new("TextLabel")
    keyStatus.Size = UDim2.new(1, -20, 0.3, 0)
    keyStatus.Position = UDim2.new(0, 10, 0.7, 0)
    keyStatus.BackgroundTransparency = 1
    keyStatus.Text = data.key and "üîë REQUIRES KEY" or "‚úÖ NO KEY"
    keyStatus.TextColor3 = data.key and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(0, 255, 0)
    keyStatus.TextSize = 11
    keyStatus.Font = Enum.Font.GothamBold
    keyStatus.TextXAlignment = Enum.TextXAlignment.Left
    keyStatus.Parent = btn
    
    local isSupported = false
    for _, id in ipairs(data.games) do
        if id == game.PlaceId then
            isSupported = true
            break
        end
    end
    
    if not isSupported and #data.games > 0 then
        TweenService:Create(btn, TweenInfoSmooth, {BackgroundTransparency = 0.7}):Play()
        title.TextColor3 = Color3.new(0.6,0.6,0.6)
        desc.TextColor3 = Color3.new(0.4,0.4,0.4)
        keyStatus.TextColor3 = Color3.new(0.4,0.4,0.4)
    end
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfoSmooth, {BackgroundTransparency = 0.3}):Play()
        TweenService:Create(stroke, TweenInfoSmooth, {Transparency = 0.5}):Play()
    end)
    
    btn.MouseLeave:Connect(function()
        local target = (isSupported or #data.games == 0) and 0.5 or 0.7
        TweenService:Create(btn, TweenInfoSmooth, {BackgroundTransparency = target}):Play()
        TweenService:Create(stroke, TweenInfoSmooth, {Transparency = 0.8}):Play()
    end)
    
    btn.MouseButton1Click:Connect(function()
        if not isSupported and #data.games > 0 then
            ShowNotif("‚ö†Ô∏è Not optimized for this game", 3)
            return
        end
        
        if data.key then
            ShowNotif("üîë Opening key link...", 2)
            if syn and syn.write_clipboard then
                syn.write_clipboard(data.keyurl)
                ShowNotif("üìã Key URL copied!", 3)
            end
        end
        
        ShowNotif("üìú Executing " .. data.name .. "...", 2)
        spawn(function()
            pcall(function()
                loadstring(data.script)()
            end)
        end)
    end)
    
    AddHover(btn)
    return btn
end

for i, scriptData in ipairs(ScriptHub) do
    CreateScriptButton(scriptData, i)
end

