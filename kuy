    InfJump.HighJump = not InfJump.HighJump
    if InfJump.HighJump then
        HighBtn.Text = "High: ON"
        HighBtn.BackgroundColor3 = Color3.fromRGB(80, 140, 80)
    else
        HighBtn.Text = "High: OFF"
        HighBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ñ‡πâ‡∏≤‡∏õ‡∏¥‡∏î
        local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum and InfJump.Enabled then hum.JumpPower = 50 end
    end
end)

