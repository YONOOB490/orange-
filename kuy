    local color = Clicker.Enabled and Color3.fromRGB(60, 160, 80) or Color3.fromRGB(180, 60, 60)
    TweenService:Create(ClickerBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

