-- ==============================================
-- ‡πÅ‡∏ó‡πá‡∏ö Settings - ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ GUI (‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏°‡∏∏‡∏°‡πÇ‡∏Ñ‡πâ‡∏á‡πÅ‡∏•‡πâ‡∏ß)
-- ==============================================

local guiSettings = {
    bgColor = Color3.fromRGB(20, 20, 28),
    titleColor = Color3.fromRGB(28, 28, 38),
    transparency = 0,
    textSizeScale = 1.0,
    strokeThickness = 1
}

local function updateGUI()
    TweenService:Create(Main, TweenInfo.new(0.4), {
        BackgroundColor3 = guiSettings.bgColor,
        BackgroundTransparency = guiSettings.transparency
    }):Play()
    
    TweenService:Create(Title, TweenInfo.new(0.4), {
        BackgroundColor3 = guiSettings.titleColor
    }):Play()
    
    Stroke.Thickness = guiSettings.strokeThickness
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó text size scale
    for _, obj in ipairs(Main:GetDescendants()) do
        if obj:IsA("TextLabel") or obj:IsA("TextButton") or obj:IsA("TextBox") then
            if obj ~= TitleText then
                obj.TextSize = obj.TextSize * guiSettings.textSizeScale
            end
        end
    end
end

local function addSettingRow(labelText, defaultValue, callback)
    local row = Instance.new("Frame")
    row.Size = UDim2.new(1, -30, 0, 38)
    row.BackgroundTransparency = 1
    row.Parent = SettingsPage

    local lbl = Instance.new("TextLabel")
    lbl.Size = UDim2.new(0.55, 0, 1, 0)
    lbl.BackgroundTransparency = 1
    lbl.Text = labelText
    lbl.TextColor3 = Color3.fromRGB(180, 180, 220)
    lbl.Font = Enum.Font.GothamMedium
    lbl.TextSize = 14
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    lbl.Parent = row

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(0.45, 0, 0.8, 0)
    input.Position = UDim2.new(0.55, 0, 0.1, 0)
    input.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
    input.TextColor3 = Color3.new(1,1,1)
    input.Text = tostring(defaultValue)
    input.Font = Enum.Font.Gotham
    input.TextSize = 13
    input.ClearTextOnFocus = false
    input.Parent = row

    -- ‡πÑ‡∏°‡πà‡∏°‡∏µ UICorner ‚Üí ‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏°

    input.FocusLost:Connect(function(enter)
        if enter then
            callback(input.Text)
            updateGUI()
        end
    end)
end

-- ‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ (‡πÑ‡∏°‡πà‡∏°‡∏µ Corner Radius ‡πÅ‡∏•‡πâ‡∏ß)
addSettingRow("‡∏™‡∏µ‡∏û‡∏∑‡πâ‡∏ô‡∏´‡∏•‡∏±‡∏á (R,G,B)", "20,20,28", function(val)
    local r,g,b = val:match("(%d+),%s*(%d+),%s*(%d+)")
    if r and g and b then guiSettings.bgColor = Color3.fromRGB(tonumber(r),tonumber(g),tonumber(b)) end
end)

addSettingRow("‡∏™‡∏µ Title Bar (R,G,B)", "28,28,38", function(val)
    local r,g,b = val:match("(%d+),%s*(%d+),%s*(%d+)")
    if r and g and b then guiSettings.titleColor = Color3.fromRGB(tonumber(r),tonumber(g),tonumber(b)) end
end)

addSettingRow("‡∏Ñ‡∏ß‡∏≤‡∏°‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™ (0-1)", "0", function(val)
    local n = tonumber(val)
    if n and n >= 0 and n <= 1 then guiSettings.transparency = n end
end)

addSettingRow("‡∏Ç‡∏ô‡∏≤‡∏î‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£ Scale (0.8-1.5)", "1.0", function(val)
    local n = tonumber(val)
    if n and n >= 0.8 and n <= 1.5 then guiSettings.textSizeScale = n end
end)

addSettingRow("‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡∏Ç‡∏≠‡∏ö (0.5-3)", "1", function(val)
    local n = tonumber(val)
    if n and n >= 0.5 and n <= 3 then guiSettings.strokeThickness = n end
end)

-- Reset Button
local resetBtn = Instance.new("TextButton")
resetBtn.Size = UDim2.new(1, -40, 0, 45)
resetBtn.BackgroundColor3 = Color3.fromRGB(100, 100, 160)
resetBtn.Text = "Reset to Default"
resetBtn.TextColor3 = Color3.new(1,1,1)
resetBtn.Font = Enum.Font.GothamBold
resetBtn.TextSize = 16
resetBtn.BorderSizePixel = 0
resetBtn.Parent = SettingsPage

resetBtn.MouseButton1Click:Connect(function()
    guiSettings = {
        bgColor = Color3.fromRGB(20, 20, 28),
        titleColor = Color3.fromRGB(28, 28, 38),
        transparency = 0,
        textSizeScale = 1.0,
        strokeThickness = 1
    }
    updateGUI()
    print("Reset GUI settings to default")
end)

