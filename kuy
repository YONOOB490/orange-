-- ==============================================
-- AIMBOT v2.0 - ‡πÄ‡∏•‡πá‡∏á‡∏ï‡∏≤‡∏° FOV ‡∏´‡∏£‡∏∑‡∏≠ ‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á (‡πÉ‡∏Å‡∏•‡πâ‡∏™‡∏∏‡∏î)
-- ==============================================

local Aimbot = {
    Enabled = false,
    Target = nil,
    Connection = nil,
    Mode = "FOV", -- "FOV" ‡∏´‡∏£‡∏∑‡∏≠ "Distance"
    Settings = {
        FOV = 150,           -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏≠‡∏ô‡πÇ‡∏´‡∏°‡∏î FOV
        MaxDistance = 200,   -- ‡πÉ‡∏ä‡πâ‡∏ï‡∏≠‡∏ô‡πÇ‡∏´‡∏°‡∏î Distance ( studs )
        Smoothness = 0.15,
        TargetPart = "Head",
        TeamCheck = true
    }
}

function Aimbot:GetClosest()
    if not LocalPlayer.Character then return nil end
    local root = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not root then return nil end
    
    local camera = workspace.CurrentCamera
    local center = Vector2.new(camera.ViewportSize.X/2, camera.ViewportSize.Y/2)
    local closest = nil
    
    -- ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å metric ‡∏ï‡∏≤‡∏°‡πÇ‡∏´‡∏°‡∏î
    local bestMetric = math.huge
    
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local char = player.Character
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            
            -- Team Check
            if self.Settings.TeamCheck and LocalPlayer.Team and player.Team then
                if LocalPlayer.Team == player.Team then continue end
            end
            
            if humanoid and humanoid.Health > 0 then
                local targetPart = char:FindFirstChild(self.Settings.TargetPart) 
                                  or char:FindFirstChild("HumanoidRootPart")
                                  or char:FindFirstChild("Head")
                
                if targetPart then
                    local screenPos, onScreen = camera:WorldToViewportPoint(targetPart.Position)
                    
                    -- ‡∏ï‡πâ‡∏≠‡∏á‡∏≠‡∏¢‡∏π‡πà‡∏ö‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô (‡πÑ‡∏°‡πà‡∏™‡∏ô‡πÉ‡∏à‡∏Ñ‡∏ô‡∏Ç‡πâ‡∏≤‡∏á‡∏´‡∏•‡∏±‡∏á)
                    if onScreen then
                        local metric = math.huge
                        local valid = false
                        
                        if self.Mode == "FOV" then
                            -- ‡πÇ‡∏´‡∏°‡∏î‡πÄ‡∏î‡∏¥‡∏°: ‡πÉ‡∏Å‡∏•‡πâ‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠ = ‡∏î‡∏µ
                            local screenDist = (Vector2.new(screenPos.X, screenPos.Y) - center).Magnitude
                            if screenDist <= self.Settings.FOV then
                                metric = screenDist
                                valid = true
                            end
                        else
                            -- ‡πÇ‡∏´‡∏°‡∏î‡πÉ‡∏´‡∏°‡πà: ‡πÉ‡∏Å‡∏•‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£ = ‡∏î‡∏µ
                            local dist3D = (root.Position - targetPart.Position).Magnitude
                            if dist3D <= self.Settings.MaxDistance then
                                metric = dist3D
                                valid = true
                            end
                        end
                        
                        if valid and metric < bestMetric then
                            bestMetric = metric
                            closest = targetPart
                        end
                    end
                end
            end
        end
    end
    
    return closest
end

function Aimbot:Update()
    if not self.Enabled then return end
    
    local target = self:GetClosest()
    if target and target.Parent then
        self.Target = target
        local camera = workspace.CurrentCamera
        
        -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì CFrame ‡πÉ‡∏´‡∏°‡πà
        local targetCF = CFrame.new(camera.CFrame.Position, target.Position)
        
        -- Smooth Lerp
        camera.CFrame = camera.CFrame:Lerp(targetCF, self.Settings.Smoothness)
    else
        self.Target = nil
    end
end

function Aimbot:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[Aimbot] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô | ‡πÇ‡∏´‡∏°‡∏î:", self.Mode)
        self.Connection = game:GetService("RunService").RenderStepped:Connect(function()
            self:Update()
        end)
    else
        print("[Aimbot] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
    end
    
    return self.Enabled
end

-- ==============================================
-- UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Aimbot v2.0 (‡∏°‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏•‡∏±‡∏ö‡πÇ‡∏´‡∏°‡∏î)
-- ==============================================

local AimFrame = Instance.new("Frame")
AimFrame.Size = UDim2.new(1, -30, 0, 140) -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡πÄ‡∏õ‡πá‡∏ô 140
