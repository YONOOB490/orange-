    local state = Clicker:Toggle()
    if state then
        ClickerBtn.Text = "ON"
        ClickerBtn.BackgroundColor3 = Color3.fromRGB(60, 160, 80)
        ClickerTitle.TextColor3 = Color3.fromRGB(150, 255, 150)
    else
        ClickerBtn.Text = "OFF"
        ClickerBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
        ClickerTitle.TextColor3 = Color3.fromRGB(100, 255, 100)
    end
end)

-- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
