-- ==============================================
-- XRAY VISION - ‡∏°‡∏≠‡∏á‡∏ú‡πà‡∏≤‡∏ô‡∏ú‡∏ô‡∏±‡∏á (‡∏™‡∏±‡πâ‡∏ô ‡πÉ‡∏ä‡πâ‡πÑ‡∏î‡πâ‡∏à‡∏£‡∏¥‡∏á)
-- ==============================================

local Xray = {
    Enabled = false,
    Objects = {}, -- [Part] = OriginalTransparency
    Connection = nil,
    Range = 100
}

function Xray:Scan()
    if not self.Enabled then return end
    if not LocalPlayer.Character then return end
    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    local pos = hrp.Position
    local processed = {}
    
    -- ‡∏´‡∏≤ Parts ‡πÉ‡∏ô‡∏£‡∏±‡∏®‡∏°‡∏µ (‡πÉ‡∏ä‡πâ FindPartsInBall ‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏ß‡πà‡∏≤)
    local parts = workspace:GetPartBoundsInRadius(pos, self.Range)
    
    for _, part in ipairs(parts) do
        if part.Anchored and part.Transparency < 0.9 then
            -- ‡∏Ç‡πâ‡∏≤‡∏°‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£, Terrain, ‡∏ô‡πâ‡∏≥
            if part:IsDescendantOf(LocalPlayer.Character) then continue end
            if part.Parent and part.Parent:FindFirstChild("Humanoid") then continue end
            if part:IsA("Terrain") or part.Name:lower():find("terrain") then continue end
            
            processed[part] = true
            
            -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ
            if not self.Objects[part] then
                self.Objects[part] = part.Transparency
            end
            
            -- ‡∏ó‡∏≥‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™
            part.Transparency = 0.75
        end
    end
    
    -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏£‡∏∞‡∏¢‡∏∞
    for part, original in pairs(self.Objects) do
        if not processed[part] then
            if part and part.Parent then
                part.Transparency = original
            end
            self.Objects[part] = nil
        end
    end
end

function Xray:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[Xray] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            if tick() % 0.5 < 0.03 then -- ‡∏™‡πÅ‡∏Å‡∏ô‡∏ó‡∏∏‡∏Å 0.5 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
                self:Scan()
            end
        end)
    else
        print("[Xray] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then self.Connection:Disconnect() end
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
        for part, original in pairs(self.Objects) do
            if part and part.Parent then
                part.Transparency = original
            end
        end
        self.Objects = {}
    end
    
    return self.Enabled
end

-- UI Xray (‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ï‡πâ Jump)
