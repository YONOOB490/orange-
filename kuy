-- ‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠
local AimTitle = Instance.new("TextLabel")
AimTitle.Size = UDim2.new(1, -20, 0, 26)
AimTitle.Position = UDim2.new(0, 10, 0, 8)
AimTitle.BackgroundTransparency = 1
AimTitle.Text = "üéØ AIMBOT v2.0"
