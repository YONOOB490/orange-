MinBtn.MouseButton1Click:Connect(function()
    TweenService:Create(Main, TweenInfo.new(0.22, Enum.EasingStyle.Quad), {
        Size = UDim2.new(0,0,0,0),
        Position = UDim2.new(0.5,0,0.5,0),
        BackgroundTransparency = 1
    }):Play()
    task.delay(0.22, function()
        Main.Visible = false
        minIcon.Visible = true
    end)
end)

minIcon.MouseButton1Click:Connect(function()
    Main.Visible = true
    Main.BackgroundTransparency = 1
    Main.Size = UDim2.new(0,0,0,0)
    Main.Position = UDim2.new(0.5,0,0.5,0)
    TweenService:Create(Main, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
        Size = normalSize,
        Position = normalPos,
        BackgroundTransparency = 0
    }):Play()
    minIcon.Visible = false
end)

-- Close
CloseBtn.MouseButton1Click:Connect(function()
    TweenService:Create(Main, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
        BackgroundTransparency = 1,
        Size = UDim2.new(0.75,0,0.75,0)
    }):Play()
    task.delay(0.2, function() sg:Destroy() end)
end)

-- Draggable
local dragging, dragStart, startPos
Title.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = Main.Position
    end
end)

Title.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = false
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStart
        Main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- Open Animation
Main.BackgroundTransparency = 1
Main.Size = UDim2.new(0.88, 0, 0.88, 0)
Main.Position = UDim2.new(0.5, -Main.AbsoluteSize.X*0.44, 0.5, -Main.AbsoluteSize.Y*0.44)
Main.Visible = true

TweenService:Create(Main, TweenInfo.new(0.35, Enum.EasingStyle.Quad), {
    BackgroundTransparency = 0,
    Size = UDim2.new(0, 400, 0, 340),
    Position = UDim2.new(0.5, -200, 0.5, -170)
}):Play()

print("Hub v12.1 Loaded ‚Äì ‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏°‡∏´‡∏°‡∏î‡∏ó‡∏∏‡∏Å‡∏à‡∏∏‡∏î‡πÅ‡∏•‡πâ‡∏ß")
-- ==============================================
-- ANTI-BAN SYSTEM v2.0 - ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÅ‡∏ö‡∏ô/‡πÄ‡∏ï‡∏∞/‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
-- ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Delta Executor (‡∏•‡∏ö‡πÇ‡∏Ñ‡πâ‡∏î‡∏ã‡πâ‡∏≥ + ‡πÉ‡∏ä‡πâ pcall)
-- ==============================================

local AntiBan = {
    Enabled = true,
    Initialized = false,
    LastCheck = tick(),
    CheckInterval = 10,
    SuspiciousActivity = 0,
    
    Settings = {
        AntiKick = true,
        AntiBan = true,
        AntiDetection = true
    }
}

-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡πÇ‡∏î‡∏ô‡πÄ‡∏ï‡∏∞
function AntiBan:AntiKick()
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    
    Players.ChildRemoved:Connect(function(child)
        if child == LocalPlayer and self.Settings.AntiKick then
            task.spawn(function()
                task.wait(0.5)
                if not LocalPlayer.Parent then
                    print("[AntiBan] ‚ö†Ô∏è Kick detected")
                end
            end)
        end
    end)
end

-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡πÅ‡∏ö‡∏ô
function AntiBan:AntiBan()
    game.DescendantAdded:Connect(function(descendant)
        if descendant:IsA("Script") or descendant:IsA("LocalScript") then
            local name = descendant.Name:lower()
            if name:find("ban") or name:find("anticheat") or name:find("security") then
                print("[AntiBan] ‚ö†Ô∏è Detected:", descendant.Name)
            end
        end
    end)
end

-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
function AntiBan:AntiRobloxDetection()
    local CoreGui = game:GetService("CoreGui")
    
    local function checkScriptAccess(script)
        if script and script.Parent then
            local parentName = script.Parent.Name:lower()
            if parentName:find("roblox") or parentName:find("core") then
                return true
            end
        end
        return false
    end
    
    game.DescendantAdded:Connect(function(descendant)
        if descendant:IsA("Script") or descendant:IsA("LocalScript") then
            if checkScriptAccess(descendant) then
                print("[AntiBan] ‚ö†Ô∏è Script scan detected")
            end
        end
    end)
end

-- ‡∏™‡∏∏‡πà‡∏°‡∏î‡∏µ‡πÄ‡∏•‡∏¢‡πå
function AntiBan:GetRandomDelay(min, max)
    return math.random((min or 0.1) * 100, (max or 0.5) * 100) / 100
end

-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏£‡∏∞‡∏ö‡∏ö
function AntiBan:HealthCheck()
    while self.Enabled do
        task.wait(self.CheckInterval)
        if not self.Enabled then break end
        
        -- ‡πÉ‡∏ä‡πâ pcall ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô error ‡∏ñ‡πâ‡∏≤ Delta ‡∏ö‡∏•‡πá‡∏≠‡∏Å gcinfo
        pcall(function()
            if gcinfo and gcinfo() > 100000 then
                collectgarbage("collect")
            end
        end)
    end
end

-- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô
function AntiBan:Enable()
    if self.Initialized then return end
    self.Initialized = true
    self.Enabled = true
    
    if self.Settings.AntiKick then self:AntiKick() end
    if self.Settings.AntiBan then self:AntiBan() end
    if self.Settings.AntiDetection then self:AntiRobloxDetection() end
    
    task.spawn(function()
        self:HealthCheck()
    end)
    
    print("[AntiBan] ‚úÖ ‡∏£‡∏∞‡∏ö‡∏ö‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô (Delta Compatible)")
end

-- ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô
function AntiBan:Disable()
    self.Enabled = false
    self.Initialized = false
    print("[AntiBan] ‚è∏Ô∏è ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö
AntiBan:Enable()

-- Cleanup
sg.Destroying:Connect(function()
    AntiBan:Disable()
    print("[AntiBan] üóëÔ∏è ‡∏•‡πâ‡∏≤‡∏á‡∏ó‡∏£‡∏±‡∏û‡∏¢‡∏≤‡∏Å‡∏£")
end)

print("[AntiBan] üöÄ Anti-Ban System Loaded (Fixed for Delta)")
