-- ==============================================
-- FLOAT / ANTI-GRAVITY - ‡∏•‡∏≠‡∏¢‡∏à‡∏≤‡∏Å‡∏û‡∏∑‡πâ‡∏ô (Layout 16)
-- ==============================================

local Float = {
    Enabled = false,
    Height = 3, -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏à‡∏≤‡∏Å‡∏û‡∏∑‡πâ‡∏ô (studs)
    Connection = nil
}

function Float:Apply()
    if not self.Enabled then return end
    local char = LocalPlayer.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not hrp or not hum then return end
    
    -- Raycast ‡∏´‡∏≤‡∏û‡∏∑‡πâ‡∏ô
    local rayOrigin = hrp.Position
    local rayDir = Vector3.new(0, -100, 0)
    local rayParams = RaycastParams.new()
    rayParams.FilterDescendantsInstances = {char}
    rayParams.FilterType = Enum.RaycastFilterType.Blacklist
    
    local result = workspace:Raycast(rayOrigin, rayDir, rayParams)
    if result then
        local groundY = result.Position.Y
        local targetY = groundY + self.Height
        local currentY = hrp.Position.Y
        
        -- ‡∏ñ‡πâ‡∏≤‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£ ‡πÉ‡∏´‡πâ‡∏ú‡∏•‡∏±‡∏Å‡∏Ç‡∏∂‡πâ‡∏ô
        if currentY < targetY then
            local vel = hrp.AssemblyLinearVelocity
            hrp.AssemblyLinearVelocity = Vector3.new(vel.X, 25, vel.Z)
            hum.PlatformStand = true
        else
            hum.PlatformStand = false
        end
    end
end

function Float:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[Float] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô (‡∏™‡∏π‡∏á " .. self.Height .. " studs)")
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            self:Apply()
        end)
    else
        print("[Float] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
        local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum.PlatformStand = false end
    end
    
    return self.Enabled
end

-- UI (LayoutOrder 16)
