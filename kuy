local function createSpeedBtn(text, posX, change)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 50, 0, 30)
    btn.Position = UDim2.new(posX, 0, 0, 40)
    btn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
    btn.Text = text
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.BorderSizePixel = 0
    btn.Parent = SpeedFrame
    
    btn.MouseButton1Click:Connect(function()
        local newSpeed = SpeedSystem.TargetSpeed + change
        SpeedSystem:SetSpeed(newSpeed)
        SpeedSlider.Text = "Speed: " .. math.floor(newSpeed)
        SpeedTitle.TextColor3 = (newSpeed > 16) and Color3.fromRGB(150, 255, 150) or Color3.fromRGB(100, 200, 255)
    end)
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(80, 80, 140)}):Play()
    end)
end

createSpeedBtn("-", 0.05, -5)
createSpeedBtn("+", 0.82, 5)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï
