-- Tab Buttons
local function createTab(name, page)
    local tab = Instance.new("TextButton")
    tab.Size = UDim2.new(0.5, -4, 1, -4)
    tab.BackgroundTransparency = 1
    tab.Text = name
    tab.TextColor3 = Color3.fromRGB(160, 160, 200)
    tab.Font = Enum.Font.GothamSemibold
    tab.TextSize = 15
    tab.Parent = TabBar

    tab.MouseButton1Click:Connect(function()
        MainPage.Visible = (page == MainPage)
        SettingsPage.Visible = (page == SettingsPage)
        TweenService:Create(tab, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(220, 220, 255)}):Play()
        for _, other in ipairs(TabBar:GetChildren()) do
            if other:IsA("TextButton") and other ~= tab then
                TweenService:Create(other, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(160, 160, 200)}):Play()
            end
        end
    end)

    return tab
end

local TabMain = createTab("Main", MainPage)
TabMain.Position = UDim2.new(0, 4, 0, 2)
local TabSettings = createTab("Settings", SettingsPage)
TabSettings.Position = UDim2.new(0.5, -2, 0, 2)

-- ==============================================
-- ‡∏û‡∏∑‡πâ‡∏ô‡∏ó‡∏µ‡πà‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÉ‡∏™‡πà‡∏ü‡∏µ‡πÄ‡∏à‡∏≠‡∏£‡πå‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î (‡πÅ‡∏ó‡πá‡∏ö Main)
-- ==============================================
-- ==============================================
-- ESP SYSTEM - ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÉ‡∏´‡∏°‡πà‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Hub v12.1
-- ==============================================

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å (‡πÉ‡∏ä‡πâ‡∏à‡∏≤‡∏Å GUI ‡∏´‡∏•‡∏±‡∏Å‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß)
local ESP = {
    Enabled = false,
    Objects = {},
    Connections = {},
    LoopConnection = nil,
    LastUpdate = 0,
    Transparency = 0.85
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πâ‡∏≤‡∏á ESP
function ESP:Clear()
    for player, data in pairs(self.Objects) do
        if data.Highlight then
            data.Highlight:Destroy()
        end
    end
    self.Objects = {}
    
    for _, conn in pairs(self.Connections) do
        if conn then conn:Disconnect() end
    end
    self.Connections = {}
    
    if self.LoopConnection then
        self.LoopConnection:Disconnect()
        self.LoopConnection = nil
    end
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á ESP ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
function ESP:Create(player)
    if player == LocalPlayer then return end
    if not player.Character then return end
    if self.Objects[player] then return end
    
    local char = player.Character
    if not char:FindFirstChild("Humanoid") then return end
    
    local hl = Instance.new("Highlight")
    hl.Name = "XESPHighlight"
    hl.Adornee = char
    hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    hl.FillColor = player.Team and player.Team.TeamColor.Color or Color3.fromRGB(255, 80, 80)
    hl.FillTransparency = self.Transparency
    hl.OutlineColor = Color3.fromRGB(255, 255, 255)
    hl.OutlineTransparency = 0.2
    
    -- ‡πÉ‡∏™‡πà‡πÉ‡∏ô CoreGui ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏´‡∏≤‡∏¢ (‡πÉ‡∏ä‡πâ pcall ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô error)
    pcall(function()
        hl.Parent = game:GetService("CoreGui")
    end)
    if not hl.Parent then
        hl.Parent = workspace.CurrentCamera
    end
    
    self.Objects[player] = {
        Highlight = hl,
        Character = char
    }
end

-- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó ESP
function ESP:Update()
    if not self.Enabled then return end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á ESP ‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            if not self.Objects[player] or self.Objects[player].Character ~= player.Character then
                self:Create(player)
            end
        end
    end
    
    -- ‡∏•‡∏ö ESP ‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ‡πÅ‡∏•‡πâ‡∏ß
    for player, data in pairs(self.Objects) do
        if not player.Parent or not data.Highlight or not data.Highlight.Parent then
            if data.Highlight then data.Highlight:Destroy() end
            self.Objects[player] = nil
        end
    end
end

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ESP
function ESP:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        self:Clear()
        print("[ESP] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        
        -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        for _, player in ipairs(Players:GetPlayers()) do
            self:Create(player)
        end
        
        -- Loop ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó
        self.LoopConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if tick() - self.LastUpdate >= 0.1 then
                self.LastUpdate = tick()
                self:Update()
            end
        end)
        
        -- Event ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà
        self.Connections.PlayerAdded = Players.PlayerAdded:Connect(function(player)
            task.delay(1, function()
                if self.Enabled then
                    self:Create(player)
                end
            end)
        end)
        
    else
        print("[ESP] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        self:Clear()
    end
    
    return self.Enabled
end

-- UI ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö ESP (‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏±‡∏ö‡∏î‡∏µ‡πÑ‡∏ã‡∏ô‡πå GUI ‡∏Ñ‡∏∏‡∏ì)
local ESPFrame = Instance.new("Frame")
ESPFrame.Size = UDim2.new(1, -30, 0, 110)
ESPFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
ESPFrame.BorderSizePixel = 0
ESPFrame.Parent = MainPage

local ESPStroke = Instance.new("UIStroke")
ESPStroke.Color = Color3.fromRGB(70, 70, 120)
ESPStroke.Thickness = 1
ESPStroke.Parent = ESPFrame

-- ‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠
local ESPTitle = Instance.new("TextLabel")
ESPTitle.Size = UDim2.new(1, -20, 0, 28)
ESPTitle.Position = UDim2.new(0, 10, 0, 8)
ESPTitle.BackgroundTransparency = 1
ESPTitle.Text = "üëÅ ESP SYSTEM"
ESPTitle.TextColor3 = Color3.fromRGB(220, 220, 255)
ESPTitle.Font = Enum.Font.GothamBold
ESPTitle.TextSize = 15
ESPTitle.TextXAlignment = Enum.TextXAlignment.Left
ESPTitle.Parent = ESPFrame

-- ‡∏õ‡∏∏‡πà‡∏° Toggle
local ESPButton = Instance.new("TextButton")
ESPButton.Size = UDim2.new(0.4, 0, 0, 32)
ESPButton.Position = UDim2.new(0.05, 0, 0, 45)
ESPButton.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
ESPButton.Text = "OFF"
ESPButton.TextColor3 = Color3.new(1, 1, 1)
ESPButton.Font = Enum.Font.GothamBold
ESPButton.TextSize = 14
ESPButton.BorderSizePixel = 0
ESPButton.Parent = ESPFrame

-- ‡∏õ‡∏∏‡πà‡∏° Toggle Trans
local TransButton = Instance.new("TextButton")
TransButton.Size = UDim2.new(0.45, 0, 0, 32)
TransButton.Position = UDim2.new(0.5, 0, 0, 45)
TransButton.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
TransButton.Text = "Transparency: 85%"
TransButton.TextColor3 = Color3.new(1, 1, 1)
TransButton.Font = Enum.Font.Gotham
TransButton.TextSize = 12
TransButton.BorderSizePixel = 0
TransButton.Parent = ESPFrame

-- ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local ESPStatus = Instance.new("TextLabel")
ESPStatus.Size = UDim2.new(1, -20, 0, 20)
ESPStatus.Position = UDim2.new(0, 10, 0, 82)
ESPStatus.BackgroundTransparency = 1
ESPStatus.Text = "‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô"
ESPStatus.TextColor3 = Color3.fromRGB(150, 150, 180)
ESPStatus.Font = Enum.Font.Gotham
ESPStatus.TextSize = 12
ESPStatus.TextXAlignment = Enum.TextXAlignment.Left
ESPStatus.Parent = ESPFrame

-- Hover Effects
ESPButton.MouseEnter:Connect(function()
    local targetColor = ESP.Enabled and Color3.fromRGB(80, 200, 100) or Color3.fromRGB(200, 80, 80)
    TweenService:Create(ESPButton, TweenInfo.new(0.15), {BackgroundColor3 = targetColor}):Play()
end)

ESPButton.MouseLeave:Connect(function()
    local targetColor = ESP.Enabled and Color3.fromRGB(60, 160, 80) or Color3.fromRGB(180, 60, 60)
    TweenService:Create(ESPButton, TweenInfo.new(0.15), {BackgroundColor3 = targetColor}):Play()
end)

TransButton.MouseEnter:Connect(function()
    TweenService:Create(TransButton, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
end)

TransButton.MouseLeave:Connect(function()
    TweenService:Create(TransButton, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(80, 80, 140)}):Play()
end)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
ESPButton.MouseButton1Click:Connect(function()
    local state = ESP:Toggle()
    
    if state then
        ESPButton.Text = "ON"
        ESPButton.BackgroundColor3 = Color3.fromRGB(60, 160, 80)
        ESPTitle.TextColor3 = Color3.fromRGB(150, 255, 150)
        ESPStatus.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô..."
        ESPStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
    else
        ESPButton.Text = "OFF"
        ESPButton.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
        ESPTitle.TextColor3 = Color3.fromRGB(220, 220, 255)
        ESPStatus.Text = "‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô"
        ESPStatus.TextColor3 = Color3.fromRGB(150, 150, 180)
    end
end)

-- ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™
TransButton.MouseButton1Click:Connect(function()
    if ESP.Transparency == 0.85 then
        ESP.Transparency = 0.6
        TransButton.Text = "Transparency: 60%"
    elseif ESP.Transparency == 0.6 then
        ESP.Transparency = 0.95
        TransButton.Text = "Transparency: 95%"
    else
        ESP.Transparency = 0.85
        TransButton.Text = "Transparency: 85%"
    end
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà
    for _, data in pairs(ESP.Objects) do
        if data.Highlight then
            data.Highlight.FillTransparency = ESP.Transparency
        end
    end
end)

-- ‡∏•‡πâ‡∏≤‡∏á‡∏ï‡∏≠‡∏ô‡∏õ‡∏¥‡∏î GUI
sg.Destroying:Connect(function()
    ESP:Clear()
    print("[ESP] ‡∏•‡πâ‡∏≤‡∏á‡∏ó‡∏£‡∏±‡∏û‡∏¢‡∏≤‡∏Å‡∏£‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢")
end)

print("[ESP] ‡∏£‡∏∞‡∏ö‡∏ö ESP ‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß")
-- ==============================================
-- ITEM ESP SYSTEM - Universal (No Physics Parts)
-- ==============================================

local ItemESP = {
    Enabled = false,
    Objects = {},
    Connections = {},
    LoopConnection = nil,
    LastUpdate = 0,
    ScanRadius = 200,
    MaxItems = 50, -- ‡∏à‡∏≥‡∏Å‡∏±‡∏î‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡∏ó‡∏µ‡πà‡πÅ‡∏™‡∏î‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡∏î‡∏†‡∏≤‡∏£‡∏∞
    Color = Color3.fromRGB(255, 215, 0),
    TextColor = Color3.fromRGB(255, 255, 200)
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏° (Heuristic ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ó‡∏∏‡∏Å‡πÅ‡∏°‡∏û)
function ItemESP:IsItem(obj)
    if not obj:IsA("Model") then return false end
    if obj.Parent == workspace:FindFirstChild("Players") or obj.Parent == game.Players then return false end
    
    -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    if game.Players:GetPlayerFromCharacter(obj) then return false end
    
    -- ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ PrimaryPart ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
    local primary = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
    if not primary then return false end
    
    -- ‡∏Ç‡∏ô‡∏≤‡∏î‡∏ï‡πâ‡∏≠‡∏á‡πÑ‡∏°‡πà‡πÉ‡∏´‡∏ç‡πà‡∏à‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ (‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà‡∏ö‡πâ‡∏≤‡∏ô/‡∏ï‡πâ‡∏ô‡πÑ‡∏°‡πâ‡πÉ‡∏´‡∏ç‡πà)
    local size = obj:GetExtentsSize()
    if size.Magnitude > 20 then return false end
    
    -- ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ Mesh ‡∏´‡∏£‡∏∑‡∏≠ Texture (‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡∏°‡∏±‡∏Å‡∏°‡∏µ‡∏•‡∏±‡∏Å‡∏©‡∏ì‡∏∞‡∏û‡∏¥‡πÄ‡∏®‡∏©)
    local hasVisual = obj:FindFirstChildWhichIsA("SpecialMesh") or 
                      obj:FindFirstChildWhichIsA("Texture") or
                      obj:FindFirstChildWhichIsA("Decal") or
                      primary:IsA("MeshPart")
    
    if not hasVisual then return false end
    
    return true
end

-- ‡∏•‡πâ‡∏≤‡∏á‡∏ó‡∏£‡∏±‡∏û‡∏¢‡∏≤‡∏Å‡∏£‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
function ItemESP:Clear()
    for item, data in pairs(self.Objects) do
        if data.Highlight then data.Highlight:Destroy() end
        if data.Billboard then data.Billboard:Destroy() end
    end
    self.Objects = {}
    
    for _, conn in pairs(self.Connections) do
        if conn then conn:Disconnect() end
    end
    self.Connections = {}
    
    if self.LoopConnection then
        self.LoopConnection:Disconnect()
        self.LoopConnection = nil
    end
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á ESP ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°
function ItemESP:Create(item)
    if self.Objects[item] then return end
    if not item.Parent then return end
    
    local primary = item.PrimaryPart or item:FindFirstChildWhichIsA("BasePart")
    if not primary then return end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight
    local hl = Instance.new("Highlight")
    hl.Name = "XItemESP"
    hl.Adornee = item
    hl.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    hl.FillColor = self.Color
    hl.FillTransparency = 0.8
    hl.OutlineColor = Color3.fromRGB(255, 255, 255)
    hl.OutlineTransparency = 0.2
    
    pcall(function()
        hl.Parent = game:GetService("CoreGui")
    end)
    if not hl.Parent then
        hl.Parent = workspace.CurrentCamera
    end
    -- PATCH: ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏Ç‡∏ô‡∏≤‡∏î‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ
local size = nil
if item.GetExtentsSize then
    pcall(function()
        size = item:GetExtentsSize()
    end)
end
if not size then
    size = primary.Size or Vector3.new(1,1,1)
end
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Label ‡πÅ‡∏™‡∏î‡∏á‡∏ä‡∏∑‡πà‡∏≠‡πÅ‡∏•‡∏∞‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
    local bb = Instance.new("BillboardGui")
    bb.Name = "XItemLabel"
    bb.Size = UDim2.new(0, 200, 0, 40)
    bb.StudsOffset = Vector3.new(0, size.Y/2 + 1.5, 0)
    bb.AlwaysOnTop = true
    bb.MaxDistance = self.ScanRadius
    bb.Adornee = primary
    
    local txt = Instance.new("TextLabel")
    txt.Size = UDim2.new(1, 0, 1, 0)
    txt.BackgroundTransparency = 1
    txt.Text = item.Name
    txt.TextColor3 = self.TextColor
    txt.TextStrokeTransparency = 0.8
    txt.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    txt.Font = Enum.Font.GothamBold
    txt.TextSize = 14
    txt.Parent = bb
    
    pcall(function()
        bb.Parent = game:GetService("CoreGui")
    end)
    if not bb.Parent then
        bb.Parent = workspace.CurrentCamera
    end
    
    -- ‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•
    self.Objects[item] = {
        Highlight = hl,
        Billboard = bb,
        TextLabel = txt,
        PrimaryPart = primary,
        Item = item,
        LastUpdate = tick()
    }
    
    -- ‡∏•‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏ñ‡πâ‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡∏´‡∏≤‡∏¢‡πÑ‡∏õ
    self.Objects[item].Connection = item.Destroying:Connect(function()
        self:Remove(item)
    end)
end

-- ‡∏•‡∏ö ESP ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°
function ItemESP:Remove(item)
    local data = self.Objects[item]
    if not data then return end
    
    if data.Connection then data.Connection:Disconnect() end
    if data.Highlight then data.Highlight:Destroy() end
    if data.Billboard then data.Billboard:Destroy() end
    
    self.Objects[item] = nil
end

-- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡πÅ‡∏•‡∏∞‡∏•‡∏ö‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡πà‡∏≤
function ItemESP:Update()
    if not self.Enabled then return end
    if not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then return end
    
    local hrp = LocalPlayer.Character.HumanoidRootPart
    local charPos = hrp.Position
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏Ç‡∏≠‡∏á‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà
    for item, data in pairs(self.Objects) do
        if not item.Parent or not data.PrimaryPart or not data.PrimaryPart.Parent then
            self:Remove(item)
        else
            -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡πÉ‡∏ô‡∏ä‡∏∑‡πà‡∏≠
            local dist = (data.PrimaryPart.Position - charPos).Magnitude
            if dist > self.ScanRadius then
                data.Billboard.Enabled = false
                data.Highlight.Enabled = false
            else
                data.Billboard.Enabled = true
                data.Highlight.Enabled = true
                data.TextLabel.Text = string.format("%s\n[%.0fm]", item.Name, dist)
            end
        end
    end
    
    -- ‡∏™‡πÅ‡∏Å‡∏ô‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡πÉ‡∏´‡∏°‡πà (‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ)
    if tick() - self.LastUpdate > 1 then
        self.LastUpdate = tick()
        local count = 0
        
        for _, obj in ipairs(workspace:GetChildren()) do
            if count >= self.MaxItems then break end
            
            if not self.Objects[obj] and self:IsItem(obj) then
                local primary = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
                if primary then
                    local dist = (primary.Position - charPos).Magnitude
                    if dist <= self.ScanRadius then
                        self:Create(obj)
                        count = count + 1
                    end
                end
            end
        end
    end
end

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
function ItemESP:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        self:Clear()
        print("[ItemESP] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô - ‡∏™‡πÅ‡∏Å‡∏ô‡∏£‡∏±‡∏®‡∏°‡∏µ "..self.ScanRadius.." studs")
        
        -- ‡∏™‡πÅ‡∏Å‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å
        self:Update()
        
        -- Loop
        self.LoopConnection = game:GetService("RunService").Heartbeat:Connect(function()
            self:Update()
        end)
        
        -- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡πÉ‡∏´‡∏°‡πà
        self.Connections.ChildAdded = workspace.ChildAdded:Connect(function(child)
            task.delay(0.3, function()
                if self.Enabled and self:IsItem(child) then
                    self:Create(child)
                end
            end)
        end)
        
    else
        print("[ItemESP] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        self:Clear()
    end
    
    return self.Enabled
end

