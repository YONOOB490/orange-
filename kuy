AimStatus.TextColor3 = Color3.fromRGB(180, 180, 220)
AimStatus.Font = Enum.Font.Gotham
AimStatus.TextSize = 11
AimStatus.TextXAlignment = Enum.TextXAlignment.Left
AimStatus.Parent = AimFrame

-- ‡∏õ‡∏∏‡πà‡∏° Toggle ‡∏´‡∏•‡∏±‡∏Å (‡∏ã‡πâ‡∏≤‡∏¢)
local AimBtn = Instance.new("TextButton")
AimBtn.Size = UDim2.new(0.45, -5, 0, 45)
AimBtn.Position = UDim2.new(0, 10, 0, 58)
AimBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
AimBtn.Text = "OFF"
AimBtn.TextColor3 = Color3.new(1, 1, 1)
AimBtn.Font = Enum.Font.GothamBold
AimBtn.TextSize = 14
AimBtn.BorderSizePixel = 0
AimBtn.Parent = AimFrame

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏•‡∏±‡∏ö‡πÇ‡∏´‡∏°‡∏î (‡∏Ç‡∏ß‡∏≤‡∏ö‡∏ô)
local ModeBtn = Instance.new("TextButton")
ModeBtn.Size = UDim2.new(0.48, -5, 0, 20)
ModeBtn.Position = UDim2.new(0.52, 0, 0, 58)
ModeBtn.BackgroundColor3 = Color3.fromRGB(80, 140, 180) -- ‡∏™‡∏µ‡∏ü‡πâ‡∏≤ = FOV Mode
ModeBtn.Text = "Mode: FOV"
ModeBtn.TextColor3 = Color3.new(1, 1, 1)
ModeBtn.Font = Enum.Font.GothamBold
ModeBtn.TextSize = 11
ModeBtn.BorderSizePixel = 0
ModeBtn.Parent = AimFrame

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πâ‡∏≤ Head/Body (‡∏Ç‡∏ß‡∏≤‡∏•‡πà‡∏≤‡∏á)
local PartBtn = Instance.new("TextButton")
PartBtn.Size = UDim2.new(0.48, -5, 0, 22)
PartBtn.Position = UDim2.new(0.52, 0, 0, 81)
PartBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
PartBtn.Text = "Target: Head"
PartBtn.TextColor3 = Color3.new(1, 1, 1)
PartBtn.Font = Enum.Font.Gotham
PartBtn.TextSize = 11
PartBtn.BorderSizePixel = 0
PartBtn.Parent = AimFrame

-- Hover Effects
AimBtn.MouseEnter:Connect(function()
    local color = Aimbot.Enabled and Color3.fromRGB(80, 200, 100) or Color3.fromRGB(200, 80, 80)
    TweenService:Create(AimBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)
AimBtn.MouseLeave:Connect(function()
    local color = Aimbot.Enabled and Color3.fromRGB(60, 160, 80) or Color3.fromRGB(180, 60, 60)
    TweenService:Create(AimBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

ModeBtn.MouseEnter:Connect(function()
    TweenService:Create(ModeBtn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 170, 200)}):Play()
end)
ModeBtn.MouseLeave:Connect(function()
    local isFOV = Aimbot.Mode == "FOV"
    local color = isFOV and Color3.fromRGB(80, 140, 180) or Color3.fromRGB(180, 140, 80)
    TweenService:Create(ModeBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

PartBtn.MouseEnter:Connect(function()
    TweenService:Create(PartBtn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
end)
PartBtn.MouseLeave:Connect(function()
    local isHead = Aimbot.Settings.TargetPart == "Head"
    local color = isHead and Color3.fromRGB(80, 80, 140) or Color3.fromRGB(140, 100, 80)
    TweenService:Create(PartBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

-- ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° Toggle
AimBtn.MouseButton1Click:Connect(function()
    local state = Aimbot:Toggle()
    
    if state then
        AimBtn.Text = "ON"
        AimBtn.BackgroundColor3 = Color3.fromRGB(60, 160, 80)
        AimTitle.TextColor3 = Color3.fromRGB(150, 255, 150)
    else
        AimBtn.Text = "OFF"
        AimBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
        AimTitle.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

-- üÜï ‡∏™‡∏•‡∏±‡∏ö‡πÇ‡∏´‡∏°‡∏î FOV / Distance
ModeBtn.MouseButton1Click:Connect(function()
    if Aimbot.Mode == "FOV" then
        -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô Distance
        Aimbot.Mode = "Distance"
        ModeBtn.Text = "Mode: Distance"
        ModeBtn.BackgroundColor3 = Color3.fromRGB(180, 140, 80) -- ‡∏™‡πâ‡∏°
        AimStatus.Text = "Mode: Closest by Distance (200 studs)"
        AimStatus.TextColor3 = Color3.fromRGB(255, 200, 100)
        print("[Aimbot] ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÇ‡∏´‡∏°‡∏î: Distance (‡∏•‡πá‡∏≠‡∏Ñ‡∏Ñ‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡∏™‡∏∏‡∏î)")
    else
        -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πá‡∏ô FOV
        Aimbot.Mode = "FOV"
        ModeBtn.Text = "Mode: FOV"
        ModeBtn.BackgroundColor3 = Color3.fromRGB(80, 140, 180) -- ‡∏ü‡πâ‡∏≤
        AimStatus.Text = "Mode: FOV (Closest to crosshair)"
        AimStatus.TextColor3 = Color3.fromRGB(180, 180, 220)
        print("[Aimbot] ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÇ‡∏´‡∏°‡∏î: FOV (‡∏•‡πá‡∏≠‡∏Ñ‡∏Ñ‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠)")
    end
end)

-- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÄ‡∏õ‡πâ‡∏≤ Head/Body
PartBtn.MouseButton1Click:Connect(function()
    if Aimbot.Settings.TargetPart == "Head" then
        Aimbot.Settings.TargetPart = "HumanoidRootPart"
        PartBtn.Text = "Target: Body"
        PartBtn.BackgroundColor3 = Color3.fromRGB(140, 100, 80)
    else
        Aimbot.Settings.TargetPart = "Head"
        PartBtn.Text = "Target: Head"
        PartBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
    end
end)

-- Cleanup
sg.Destroying:Connect(function()
    Aimbot.Enabled = false
    if Aimbot.Connection then Aimbot.Connection:Disconnect() end
end)

print("[Aimbot] Aimbot v2.0 Loaded")
print("[Aimbot] Modes: FOV (screen center) | Distance (3D proximity)")
-- ==============================================
-- TELEPORT TO PLAYER - ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
-- ==============================================

local Teleport = {
    Enabled = false,
    Window = nil,
    ListButtons = {},
    Connection = nil
}

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï (‡∏°‡∏µ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô)
function Teleport:To(player)
    if not player or player == LocalPlayer then return end
    if not player.Character then 
        print("[Teleport] ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£")
        return 
    end
    
    local targetHRP = player.Character:FindFirstChild("HumanoidRootPart")
    local myChar = LocalPlayer.Character
    local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")
    
    if not targetHRP or not myHRP then return end
    
    -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á: ‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏•‡∏±‡∏á 5 studs + ‡∏™‡∏π‡∏á‡∏Ç‡∏∂‡πâ‡∏ô 3 studs (‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏ï‡∏¥‡∏î‡∏û‡∏∑‡πâ‡∏ô)
    local destination = targetHRP.CFrame * CFrame.new(0, 0, 6) + Vector3.new(0, 3, 0)
    
    pcall(function()
        myHRP.CFrame = destination
        -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏î‡πá‡∏ô‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
        myHRP.Velocity = Vector3.new(0, 0, 0)
    end)
    
    print("[Teleport] ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏´‡∏≤: " .. player.Name)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
function Teleport:ToClosest()
    local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not myHRP then return end
    
    local closest = nil
    local minDist = math.huge
    
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local dist = (myHRP.Position - hrp.Position).Magnitude
                if dist < minDist then
                    minDist = dist
                    closest = player
                end
            end
        end
    end
    
    if closest then
        self:To(closest)
    else
        print("[Teleport] ‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô")
    end
end

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠ (Popup)
function Teleport:OpenWindow()
    if self.Window and self.Window.Parent then
        self.Window:Destroy()
    end
    
    -- ‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏•‡∏±‡∏Å (‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠)
    self.Window = Instance.new("Frame")
    self.Window.Size = UDim2.new(0, 280, 0, 350)
    self.Window.Position = UDim2.new(0.5, -140, 0.5, -175)
    self.Window.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
    self.Window.BorderSizePixel = 0
    self.Window.Parent = sg
    
    local stroke = Instance.new("UIStroke", self.Window)
    stroke.Color = Color3.fromRGB(150, 100, 255)
    stroke.Thickness = 2
    
    -- ‡∏´‡∏±‡∏ß‡∏Ç‡πâ‡∏≠ + ‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î
    local header = Instance.new("Frame")
    header.Size = UDim2.new(1, 0, 0, 40)
    header.BackgroundColor3 = Color3.fromRGB(45, 45, 65)
    header.BorderSizePixel = 0
    header.Parent = self.Window
    
    local title = Instance.new("TextLabel", header)
    title.Size = UDim2.new(1, -50, 1, 0)
    title.Position = UDim2.new(0, 15, 0, 0)
    title.BackgroundTransparency = 1
    title.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô"
    title.TextColor3 = Color3.fromRGB(200, 150, 255)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 16
    title.TextXAlignment = Enum.TextXAlignment.Left
    
    local closeBtn = Instance.new("TextButton", header)
    closeBtn.Size = UDim2.new(0, 36, 0, 36)
    closeBtn.Position = UDim2.new(1, -40, 0, 2)
    closeBtn.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
    closeBtn.Text = "√ó"
    closeBtn.TextColor3 = Color3.new(1, 1, 1)
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.TextSize = 20
    closeBtn.BorderSizePixel = 0
    
    closeBtn.MouseButton1Click:Connect(function()
        self.Window:Destroy()
        self.Enabled = false
        TeleportBtn.Text = "OPEN LIST"
        TeleportBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
    end)
    
    -- ScrollingList
    local scroll = Instance.new("ScrollingFrame", self.Window)
    scroll.Size = UDim2.new(1, -20, 1, -100)
    scroll.Position = UDim2.new(0, 10, 0, 50)
    scroll.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    scroll.BorderSizePixel = 0
    scroll.ScrollBarThickness = 6
    scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
    
    local listLayout = Instance.new("UIListLayout", scroll)
    listLayout.Padding = UDim.new(0, 5)
    listLayout.SortOrder = Enum.SortOrder.LayoutOrder
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    local function refreshList()
        for _, btn in ipairs(self.ListButtons) do
            btn:Destroy()
        end
        self.ListButtons = {}
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                local btn = Instance.new("TextButton")
                btn.Size = UDim2.new(1, -10, 0, 32)
                btn.BackgroundColor3 = Color3.fromRGB(60, 60, 90)
                btn.Text = "  " .. player.Name
                btn.TextColor3 = Color3.new(1, 1, 1)
                btn.Font = Enum.Font.Gotham
                btn.TextSize = 14
                btn.TextXAlignment = Enum.TextXAlignment.Left
                btn.Parent = scroll
                
                btn.MouseEnter:Connect(function()
                    TweenService:Create(btn, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(80, 80, 120)}):Play()
                end)
                btn.MouseLeave:Connect(function()
                    TweenService:Create(btn, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(60, 60, 90)}):Play()
                end)
                
                btn.MouseButton1Click:Connect(function()
                    self:To(player)
                    btn.BackgroundColor3 = Color3.fromRGB(80, 180, 80)
                    btn.Text = "‚úì " .. player.Name
                    task.wait(0.5)
                    btn.BackgroundColor3 = Color3.fromRGB(60, 60, 90)
                    btn.Text = "  " .. player.Name
                end)
                
                table.insert(self.ListButtons, btn)
            end
        end
    end
    
    refreshList()
    
    -- ‡∏õ‡∏∏‡πà‡∏°‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä + ‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
    local btnFrame = Instance.new("Frame", self.Window)
    btnFrame.Size = UDim2.new(1, -20, 0, 40)
    btnFrame.Position = UDim2.new(0, 10, 1, -50)
    btnFrame.BackgroundTransparency = 1
    
    local refreshBtn = Instance.new("TextButton", btnFrame)
    refreshBtn.Size = UDim2.new(0.48, -5, 1, 0)
    refreshBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
    refreshBtn.Text = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä"
    refreshBtn.TextColor3 = Color3.new(1, 1, 1)
    refreshBtn.Font = Enum.Font.GothamBold
    refreshBtn.TextSize = 13
    refreshBtn.BorderSizePixel = 0
    
    refreshBtn.MouseButton1Click:Connect(function()
        refreshList()
        refreshBtn.Text = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡πÅ‡∏•‡πâ‡∏ß!"
        task.wait(0.5)
        refreshBtn.Text = "‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä"
    end)
    
    local closestBtn = Instance.new("TextButton", btnFrame)
    closestBtn.Size = UDim2.new(0.48, -5, 1, 0)
    closestBtn.Position = UDim2.new(0.52, 0, 0, 0)
    closestBtn.BackgroundColor3 = Color3.fromRGB(80, 140, 80)
    closestBtn.Text = "‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î"
    closestBtn.TextColor3 = Color3.new(1, 1, 1)
    closestBtn.Font = Enum.Font.GothamBold
    closestBtn.TextSize = 13
    closestBtn.BorderSizePixel = 0
    
    closestBtn.MouseButton1Click:Connect(function()
        self:ToClosest()
        closestBtn.Text = "‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÅ‡∏•‡πâ‡∏ß!"
        task.wait(0.5)
        closestBtn.Text = "‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î"
    end)
    
    -- ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏Ñ‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤/‡∏≠‡∏≠‡∏Å
    self.Connection = Players.PlayerAdded:Connect(refreshList)
    Players.PlayerRemoving:Connect(refreshList)
end

-- UI ‡∏´‡∏•‡∏±‡∏Å‡πÉ‡∏ô‡πÅ‡∏ó‡πá‡∏ö Main (‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î)
local TPFrame = Instance.new("Frame")
TPFrame.Size = UDim2.new(1, -30, 0, 90)
TPFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
TPFrame.BorderSizePixel = 0
TPFrame.LayoutOrder = 8 -- ‡∏ï‡πà‡∏≠‡∏à‡∏≤‡∏Å Aimbot
TPFrame.Parent = MainPage

local TPStroke = Instance.new("UIStroke", TPFrame)
TPStroke.Color = Color3.fromRGB(70, 70, 120)
TPStroke.Thickness = 1

local TPTitle = Instance.new("TextLabel")
TPTitle.Size = UDim2.new(1, -20, 0, 26)
TPTitle.Position = UDim2.new(0, 10, 0, 8)
TPTitle.BackgroundTransparency = 1
TPTitle.Text = "‚ö° TELEPORT"
TPTitle.TextColor3 = Color3.fromRGB(150, 100, 255)
TPTitle.Font = Enum.Font.GothamBold
TPTitle.TextSize = 15
TPTitle.TextXAlignment = Enum.TextXAlignment.Left
TPTitle.Parent = TPFrame

local TPSubtitle = Instance.new("TextLabel")
TPSubtitle.Size = UDim2.new(1, -20, 0, 20)
TPSubtitle.Position = UDim2.new(0, 10, 0, 30)
TPSubtitle.BackgroundTransparency = 1
TPSubtitle.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏´‡∏≤‡∏ó‡∏±‡∏ô‡∏ó‡∏µ"
TPSubtitle.TextColor3 = Color3.fromRGB(180, 180, 220)
TPSubtitle.Font = Enum.Font.Gotham
TPSubtitle.TextSize = 11
TPSubtitle.TextXAlignment = Enum.TextXAlignment.Left
TPSubtitle.Parent = TPFrame

local TeleportBtn = Instance.new("TextButton")
TeleportBtn.Size = UDim2.new(0.9, 0, 0, 32)
TeleportBtn.Position = UDim2.new(0.05, 0, 0, 52)
TeleportBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
TeleportBtn.Text = "OPEN LIST"
TeleportBtn.TextColor3 = Color3.new(1, 1, 1)
TeleportBtn.Font = Enum.Font.GothamBold
TeleportBtn.TextSize = 14
TeleportBtn.BorderSizePixel = 0
TeleportBtn.Parent = TPFrame

-- Hover Effects
TeleportBtn.MouseEnter:Connect(function()
    TweenService:Create(TeleportBtn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
end)
TeleportBtn.MouseLeave:Connect(function()
    local color = Teleport.Enabled and Color3.fromRGB(60, 160, 80) or Color3.fromRGB(80, 80, 140)
    TweenService:Create(TeleportBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

-- ‡∏Å‡∏î‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
TeleportBtn.MouseButton1Click:Connect(function()
    Teleport.Enabled = not Teleport.Enabled
    
    if Teleport.Enabled then
        TeleportBtn.Text = "CLOSE LIST"
        TeleportBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
        Teleport:OpenWindow()
    else
        TeleportBtn.Text = "OPEN LIST"
        TeleportBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
        if Teleport.Window then Teleport.Window:Destroy() end
    end
end)

-- ‡∏Ñ‡∏µ‡∏¢‡πå‡∏•‡∏±‡∏î (PC ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô) - T = ‡πÉ‡∏Å‡∏•‡πâ‡∏™‡∏∏‡∏î
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.T then
        Teleport:ToClosest()
    end
end)

-- Cleanup
sg.Destroying:Connect(function()
    if Teleport.Connection then Teleport.Connection:Disconnect() end
    if Teleport.Window then Teleport.Window:Destroy() end
end)

print("[Teleport] Teleport System Loaded (PC: Press T for closest)")
-- ==============================================
-- INFINITE JUMP - ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏î‡πâ‡πÑ‡∏°‡πà‡∏à‡∏≥‡∏Å‡∏±‡∏î (Mobile/PC)
-- ==============================================

local InfJump = {
    Enabled = false,
    Connection = nil,
    HighJump = false
}

function InfJump:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[InfJump] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        
        -- ‡πÉ‡∏ä‡πâ JumpRequest (‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡πâ‡∏á PC ‡πÅ‡∏•‡∏∞‡∏°‡∏∑‡∏≠‡∏ñ‡∏∑‡∏≠)
        self.Connection = game:GetService("UserInputService").JumpRequest:Connect(function()
            local char = LocalPlayer.Character
            local hum = char and char:FindFirstChildOfClass("Humanoid")
            if hum then
                -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÄ‡∏õ‡πá‡∏ô Jumping (‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÑ‡∏î‡πâ‡πÅ‡∏°‡πâ‡∏≠‡∏¢‡∏π‡πà‡∏Å‡∏•‡∏≤‡∏á‡∏≠‡∏≤‡∏Å‡∏≤‡∏®)
                pcall(function()
                    hum:ChangeState(Enum.HumanoidStateType.Jumping)
                    if self.HighJump then
                        hum.JumpPower = 80
                    else
                        hum.JumpPower = 50
                    end
                end)
            end
        end)
    else
        print("[InfJump] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤ JumpPower
        local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then hum.JumpPower = 50 end
    end
    
    return self.Enabled
end

