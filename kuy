        Status.TextColor3 = Color3.fromRGB(100, 255, 100)
    else
        HitBtn.Text = "OFF"
        HitBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
        HitTitle.TextColor3 = Color3.fromRGB(255, 100, 100)
        Status.Text = "‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢: ‡∏®‡∏±‡∏ï‡∏£‡∏π‡πÉ‡∏ô‡∏ó‡∏µ‡∏°‡∏≠‡∏∑‡πà‡∏ô"
        Status.TextColor3 = Color3.fromRGB(150, 150, 180)
    end
end)

-- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ç‡∏ô‡∏≤‡∏î
SizeBtn.MouseButton1Click:Connect(function()
    local sizes = {2, 3, 5, 10}
    local idx = table.find(sizes, Hitbox.Size) or 1
    idx = idx % #sizes + 1
    Hitbox.Size = sizes[idx]
    SizeBtn.Text = "Size: x" .. Hitbox.Size
    
    -- ‡∏£‡∏µ‡πÄ‡∏ü‡∏£‡∏ä‡∏ñ‡πâ‡∏≤‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
    if Hitbox.Enabled then
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                Hitbox:ProcessCharacter(player.Character)
            end
        end
    end
end)

SizeBtn.MouseEnter:Connect(function()
    TweenService:Create(SizeBtn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
end)
SizeBtn.MouseLeave:Connect(function()
    TweenService:Create(SizeBtn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(80, 80, 140)}):Play()
end)

-- ‡∏ï‡∏¥‡∏î‡∏ï‡∏≤‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà
Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(char)
        if Hitbox.Enabled then
            task.wait(1)
            Hitbox:ProcessCharacter(char)
        end
    end)
end)

-- Cleanup
sg.Destroying:Connect(function()
    Hitbox.Enabled = false
    if Hitbox.Connection then Hitbox.Connection:Disconnect() end
    -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    for part, data in pairs(Hitbox.Originals) do
        if part and part.Parent then
            part.Size = data.Size
            part.Transparency = data.Transparency
            part.Material = data.Material
        end
    end
end)

