-- ==============================================
-- AUTO CLICKER - ‡∏Ñ‡∏•‡∏¥‡∏Å/‡∏ï‡∏µ‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÉ‡∏ä‡πâ‡πÑ‡∏î‡πâ‡∏à‡∏£‡∏¥‡∏á)
-- ==============================================

local Clicker = {
    Enabled = false,
    Speed = 10, -- ‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ï‡πà‡∏≠‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    Mode = "Tool", -- "Tool" ‡∏´‡∏£‡∏∑‡∏≠ "Mouse"
    Connection = nil,
    LastClick = 0
}

function Clicker:Click()
    if self.Mode == "Tool" then
        -- ‡πÉ‡∏ä‡πâ Tool (‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏Å‡∏±‡∏ö‡πÄ‡∏Å‡∏°‡∏ï‡∏µ/‡∏ü‡∏±‡∏ô)
        local char = LocalPlayer.Character
        local tool = char and char:FindFirstChildOfClass("Tool")
        if tool then
            pcall(function()
                tool:Activate()
            end)
        end
    else
        -- ‡πÉ‡∏ä‡πâ Mouse (PC ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô)
        local mouse = LocalPlayer:GetMouse()
        if mouse then
            pcall(function()
                mouse1click()
            end)
        end
    end
end

function Clicker:Update()
    if not self.Enabled then return end
    
    local now = tick()
    local interval = 1 / self.Speed
    
    if now - self.LastClick >= interval then
        self.LastClick = now
        self:Click()
    end
end

function Clicker:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[Clicker] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô (" .. self.Speed .. " CPS)")
        
        -- ‡πÉ‡∏ä‡πâ Heartbeat (‡πÑ‡∏°‡πà‡∏Ñ‡∏ß‡∏£‡∏°‡∏µ task.wait ‡∏Ç‡πâ‡∏≤‡∏á‡πÉ‡∏ô)
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            self:Update()
        end)
    else
        print("[Clicker] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
    end
    
    return self.Enabled
end

-- UI (LayoutOrder 12 ‡∏ï‡πà‡∏≠‡∏à‡∏≤‡∏Å Full Bright)
local ClickerFrame = Instance.new("Frame")
ClickerFrame.Size = UDim2.new(1, -30, 0, 120)
