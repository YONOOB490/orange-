-- ==============================================
-- SPEED CONTROL - ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏•‡∏∑‡πà‡∏ô‡πÑ‡∏´‡∏•
-- ==============================================

local SpeedSystem = {
    Enabled = false,
    DefaultSpeed = 16,
    TargetSpeed = 16,
    CurrentSpeed = 16,
    Connection = nil
}

function SpeedSystem:SetSpeed(value)
    self.TargetSpeed = math.clamp(value, 1, 200)
    self.Enabled = (value ~= self.DefaultSpeed)
    
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏° Loop ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ
    if not self.Connection then
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function(dt)
            if not self.Enabled then return end
            local char = LocalPlayer.Character
            if not char then return end
            local hum = char:FindFirstChildOfClass("Humanoid")
            if not hum then return end
            
            -- Tween ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏ö‡∏ö smooth (Lerp)
            self.CurrentSpeed = self.CurrentSpeed + (self.TargetSpeed - self.CurrentSpeed) * 0.1
            hum.WalkSpeed = self.CurrentSpeed
        end)
    end
end

function SpeedSystem:Reset()
    self:SetSpeed(self.DefaultSpeed)
    if self.Connection then
        self.Connection:Disconnect()
        self.Connection = nil
    end
end

-- UI Speed
