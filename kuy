    local color = Hitbox.Enabled and Color3.fromRGB(80, 200, 100) or Color3.fromRGB(200, 80, 80)
    TweenService:Create(HitBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)
HitBtn.MouseLeave:Connect(function()
    local color = Hitbox.Enabled and Color3.fromRGB(60, 160, 80) or Color3.fromRGB(180, 60, 60)
    TweenService:Create(HitBtn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play()
end)

