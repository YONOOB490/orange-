-- ==============================================
-- JUMP POWER - ‡∏õ‡∏£‡∏±‡∏ö‡πÅ‡∏£‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á
-- ==============================================

local JumpSystem = {
    Enabled = false,
    DefaultPower = 50,
    TargetPower = 50,
    Connection = nil
}

function JumpSystem:SetPower(value)
    self.TargetPower = math.clamp(value, 1, 300)
    self.Enabled = (value ~= self.DefaultPower)
    
    if not self.Connection then
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            if not self.Enabled then return end
            local char = LocalPlayer.Character
            if not char then return end
            local hum = char:FindFirstChildOfClass("Humanoid")
            if not hum then return end
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô JumpPower ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á tween ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÄ‡∏õ‡πá‡∏ô‡πÄ‡∏´‡∏ï‡∏∏‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ó‡∏±‡∏ô‡∏ó‡∏µ)
            if hum.JumpPower ~= self.TargetPower then
                hum.JumpPower = self.TargetPower
            end
        end)
    end
end

function JumpSystem:Reset()
    self:SetPower(self.DefaultPower)
    if self.Connection then
        self.Connection:Disconnect()
        self.Connection = nil
    end
end

-- UI Jump
local JumpFrame = Instance.new("Frame")
JumpFrame.Size = UDim2.new(1, -30, 0, 100)
JumpFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
JumpFrame.BorderSizePixel = 0
JumpFrame.LayoutOrder = 4
JumpFrame.Parent = MainPage

local JumpStroke = Instance.new("UIStroke", JumpFrame)
JumpStroke.Color = Color3.fromRGB(70, 70, 120)
JumpStroke.Thickness = 1

local JumpTitle = Instance.new("TextLabel")
JumpTitle.Size = UDim2.new(1, -20, 0, 24)
JumpTitle.Position = UDim2.new(0, 10, 0, 8)
JumpTitle.BackgroundTransparency = 1
JumpTitle.Text = "ü¶ò JUMP POWER"
JumpTitle.TextColor3 = Color3.fromRGB(255, 150, 100)
JumpTitle.Font = Enum.Font.GothamBold
JumpTitle.TextSize = 15
JumpTitle.TextXAlignment = Enum.TextXAlignment.Left
JumpTitle.Parent = JumpFrame

-- ‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏£‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
local JumpSlider = Instance.new("TextLabel")
JumpSlider.Size = UDim2.new(1, -120, 0, 30)
JumpSlider.Position = UDim2.new(0, 60, 0, 40)
JumpSlider.BackgroundColor3 = Color3.fromRGB(60, 60, 90)
JumpSlider.Text = "Power: 50"
JumpSlider.TextColor3 = Color3.new(1, 1, 1)
JumpSlider.Font = Enum.Font.Gotham
JumpSlider.TextSize = 13
JumpSlider.BorderSizePixel = 0
JumpSlider.Parent = JumpFrame

local function createJumpBtn(text, offset, power)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 50, 0, 30)
    btn.Position = UDim2.new(0.05, 0, 0, 40)
    btn.Position = offset and UDim2.new(0.82, 0, 0, 40) or UDim2.new(0.05, 0, 0, 40)
    btn.BackgroundColor3 = Color3.fromRGB(80, 80, 140)
    btn.Text = text
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.BorderSizePixel = 0
    btn.Parent = JumpFrame
    
    btn.MouseButton1Click:Connect(function()
        JumpSystem:SetPower(power)
        JumpSlider.Text = "Power: " .. power
        JumpTitle.TextColor3 = (power > 50) and Color3.fromRGB(150, 255, 150) or Color3.fromRGB(255, 150, 100)
    end)
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(100, 100, 170)}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = Color3.fromRGB(80, 80, 140)}):Play()
    end)
end

