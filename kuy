-- Executor Hub v12.1 - Complete GUI (‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏° ‡πÑ‡∏°‡πà‡πÇ‡∏Ñ‡πâ‡∏á‡πÄ‡∏•‡∏¢)
-- ‡∏•‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÇ‡∏Ñ‡πâ‡∏á‡∏°‡∏∏‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î + ‡∏•‡∏ö‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏õ‡∏£‡∏±‡∏ö‡∏°‡∏∏‡∏°‡πÉ‡∏ô Settings

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
-- INSERT: ‡∏£‡∏≠ LocalPlayer ‡πÉ‡∏´‡πâ‡∏û‡∏£‡πâ‡∏≠‡∏°
if not LocalPlayer then
    repeat task.wait() LocalPlayer = Players.LocalPlayer until LocalPlayer
end
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local sg = Instance.new("ScreenGui")
sg.Name = "HubFinalNoCorner"
sg.ResetOnSpawn = false
sg.Parent = LocalPlayer:WaitForChild("PlayerGui")

-- Main Frame (‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏° = 0)
local Main = Instance.new("Frame")
Main.Size = UDim2.new(0, 400, 0, 340)
Main.Position = UDim2.new(0.5, -200, 0.5, -170)
Main.BackgroundColor3 = Color3.fromRGB(20, 20, 28)
Main.BorderSizePixel = 0
Main.ClipsDescendants = true
Main.Visible = true
Main.Parent = sg

-- ‡πÑ‡∏°‡πà‡∏°‡∏µ UICorner ‡πÅ‡∏•‡πâ‡∏ß ‚Üí ‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏°‡∏ï‡∏£‡∏á

local Stroke = Instance.new("UIStroke")
Stroke.Color = Color3.fromRGB(70, 70, 120)
Stroke.Transparency = 0.75
Stroke.Thickness = 1
Stroke.Parent = Main

-- Title Bar (‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏°)
local Title = Instance.new("Frame")
Title.Size = UDim2.new(1, 0, 0, 36)
Title.BackgroundColor3 = Color3.fromRGB(28, 28, 38)
Title.BorderSizePixel = 0
Title.Parent = Main

local TitleText = Instance.new("TextLabel")
TitleText.Size = UDim2.new(1, -120, 1, 0)
TitleText.Position = UDim2.new(0, 14, 0, 0)
TitleText.BackgroundTransparency = 1
TitleText.Text = "YOnoOb Hub"
TitleText.TextColor3 = Color3.fromRGB(210, 210, 240)
TitleText.Font = Enum.Font.GothamSemibold
TitleText.TextSize = 16
TitleText.TextXAlignment = Enum.TextXAlignment.Left
TitleText.Parent = Title

-- Control Buttons (‡∏°‡∏∏‡∏°‡∏Å‡∏•‡∏°‡πÄ‡∏î‡∏¥‡∏°‡πÑ‡∏ß‡πâ ‡πÄ‡∏û‡∏£‡∏≤‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏•‡πá‡∏Å ‡πÜ ‡∏î‡∏π‡∏î‡∏µ‡∏Å‡∏ß‡πà‡∏≤)
local function createBtn(posX, color, hover, text)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 28, 0, 28)
    btn.Position = UDim2.new(1, posX, 0, 4)
    btn.BackgroundColor3 = color
    btn.Text = text
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.BorderSizePixel = 0
    btn.Parent = Title

    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(1, 0)  -- ‡∏õ‡∏∏‡πà‡∏°‡∏¢‡∏±‡∏á‡∏Å‡∏•‡∏°‡∏≠‡∏¢‡∏π‡πà
    c.Parent = btn

    btn.MouseEnter:Connect(function() TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = hover}):Play() end)
    btn.MouseLeave:Connect(function() TweenService:Create(btn, TweenInfo.new(0.15), {BackgroundColor3 = color}):Play() end)

    return btn
end

local MinBtn   = createBtn(-94, Color3.fromRGB(220, 160, 0), Color3.fromRGB(240, 180, 0), "-")
local MaxBtn   = createBtn(-62, Color3.fromRGB(0, 160, 100), Color3.fromRGB(0, 190, 120), "‚ñ°")
local CloseBtn = createBtn(-30, Color3.fromRGB(200, 60, 60), Color3.fromRGB(230, 80, 80), "√ó")
-- INSERT: ‡πÉ‡∏´‡πâ‡∏õ‡∏∏‡πà‡∏° Close ‡∏ã‡πà‡∏≠‡∏ô GUI ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î
CloseBtn.MouseButton1Click:Connect(function()
    Main.Visible = false
end)

-- Tab Bar (‡∏°‡∏∏‡∏°‡πÄ‡∏´‡∏•‡∏µ‡πà‡∏¢‡∏°)
local TabBar = Instance.new("Frame")
