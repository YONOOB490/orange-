ClickerTitle.Text = "‚ö° AUTO CLICKER"
ClickerTitle.TextColor3 = Color3.fromRGB(100, 255, 100)
ClickerTitle.Font = Enum.Font.GothamBold
ClickerTitle.TextSize = 15
ClickerTitle.TextXAlignment = Enum.TextXAlignment.Left
ClickerTitle.Parent = ClickerFrame

local ClickerSub = Instance.new("TextLabel")
ClickerSub.Size = UDim2.new(1, -20, 0, 20)
ClickerSub.Position = UDim2.new(0, 10, 0, 32)
ClickerSub.BackgroundTransparency = 1
ClickerSub.Text = "‡πÇ‡∏´‡∏°‡∏î Tool (‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏Å‡∏±‡∏ö‡πÄ‡∏Å‡∏°‡∏ï‡∏µ/‡∏ü‡∏±‡∏ô)"
ClickerSub.TextColor3 = Color3.fromRGB(180, 180, 220)
