-- ==============================================
-- NOCLIP - ‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏‡∏ú‡∏ô‡∏±‡∏á (‡∏™‡∏±‡πâ‡∏ô ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ö‡∏¥‡∏ô ‡πÑ‡∏°‡πà‡∏¢‡∏∏‡πà‡∏á‡∏¢‡∏≤‡∏Å)
-- ==============================================

local Noclip = {
    Enabled = false,
    Connection = nil
}

function Noclip:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[Noclip] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        
        self.Connection = game:GetService("RunService").Stepped:Connect(function()
            if not self.Enabled then return end
            local char = LocalPlayer.Character
            if not char then return end
            
            -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô‡∏ó‡∏∏‡∏Å‡∏™‡πà‡∏ß‡∏ô‡πÉ‡∏ô‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            for _, part in ipairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)
    else
        print("[Noclip] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
        
        -- ‡∏Ñ‡∏∑‡∏ô‡∏Ñ‡πà‡∏≤‡∏Å‡∏≤‡∏£‡∏ä‡∏ô (‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà)
        local char = LocalPlayer.Character
        if char then
            for _, part in ipairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
    end
    
    return self.Enabled
end

