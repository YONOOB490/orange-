-- ==============================================
-- FULL BRIGHT - ‡∏™‡∏ß‡πà‡∏≤‡∏á‡∏ó‡∏±‡πâ‡∏á‡πÅ‡∏°‡∏û (‡∏•‡∏ö‡πÄ‡∏á‡∏≤ ‡∏•‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏°‡∏∑‡∏î)
-- ==============================================

local FullBright = {
    Enabled = false,
    Connection = nil,
    Original = {} -- ‡πÄ‡∏Å‡πá‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°
}

function FullBright:SaveOriginal()
    local lighting = game:GetService("Lighting")
    self.Original = {
        Brightness = lighting.Brightness,
        ClockTime = lighting.ClockTime,
        FogEnd = lighting.FogEnd,
        GlobalShadows = lighting.GlobalShadows,
        Ambient = lighting.Ambient,
        OutdoorAmbient = lighting.OutdoorAmbient
    }
end

function FullBright:Apply()
    local lighting = game:GetService("Lighting")
    
    -- ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏Ñ‡πà‡∏≤‡πÄ‡∏î‡∏¥‡∏°‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ
    if not self.Original.Brightness then
        self:SaveOriginal()
    end
    
    -- ‡∏õ‡∏£‡∏±‡∏ö‡πÉ‡∏´‡πâ‡∏™‡∏ß‡πà‡∏≤‡∏á‡∏™‡∏∏‡∏î‡πÜ
    lighting.Brightness = 10
    lighting.ClockTime = 12 -- ‡πÄ‡∏ó‡∏µ‡πà‡∏¢‡∏á‡∏ß‡∏±‡∏ô
    lighting.FogEnd = 100000 -- ‡∏•‡∏ö‡∏´‡∏°‡∏≠‡∏Å
    lighting.GlobalShadows = false -- ‡∏•‡∏ö‡πÄ‡∏á‡∏≤
    lighting.Ambient = Color3.fromRGB(255, 255, 255) -- ‡πÅ‡∏™‡∏á‡∏£‡∏≠‡∏ö‡∏Ç‡πâ‡∏≤‡∏á‡∏Ç‡∏≤‡∏ß‡∏™‡∏∏‡∏î
    lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
end

function FullBright:Restore()
    local lighting = game:GetService("Lighting")
    if self.Original.Brightness then
        lighting.Brightness = self.Original.Brightness
        lighting.ClockTime = self.Original.ClockTime
        lighting.FogEnd = self.Original.FogEnd
        lighting.GlobalShadows = self.Original.GlobalShadows
        lighting.Ambient = self.Original.Ambient
        lighting.OutdoorAmbient = self.Original.OutdoorAmbient
    end
end

function FullBright:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[FullBright] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        self:Apply()
        
        -- Loop ‡∏Ñ‡∏≠‡∏¢‡∏î‡∏±‡∏ô‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡∏°‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Å‡∏•‡∏±‡∏ö (‡∏ö‡∏≤‡∏á‡πÄ‡∏Å‡∏°‡∏°‡∏µ‡∏£‡∏∞‡∏ö‡∏ö‡∏ß‡∏±‡∏ô/‡∏Ñ‡∏∑‡∏ô)
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            if self.Enabled then
                local lighting = game:GetService("Lighting")
                -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏Å‡∏°‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô ‡πÉ‡∏´‡πâ‡∏î‡∏±‡∏ô‡∏Å‡∏•‡∏±‡∏ö
                if lighting.Brightness < 5 then
                    lighting.Brightness = 10
                end
                if lighting.GlobalShadows then
                    lighting.GlobalShadows = false
                end
            end
        end)
        
    else
        print("[FullBright] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
        self:Restore()
    end
    
    return self.Enabled
end

-- UI Full Bright (LayoutOrder 11 ‡∏ï‡πà‡∏≠‡∏à‡∏≤‡∏Å Sound Hub)
