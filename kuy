-- ==============================================
-- NPC WARP - ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤ NPC/Bot (Layout 13)
-- ==============================================

local NPCWarp = {
    Enabled = false,
    Mode = "Interact", -- Interact, Attack, Follow
    Range = 200,
    Connection = nil,
    LastScan = 0,
    Targets = {"NPC", "Bot", "Mob", "Enemy", "Monster", "Villager", "Quest"}
}

function NPCWarp:Find()
    if not LocalPlayer.Character then return nil end
    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return nil end
    
    for _, obj in ipairs(workspace:GetChildren()) do
        if obj:IsA("Model") and obj ~= LocalPlayer.Character then
            local name = obj.Name:lower()
            for _, target in ipairs(self.Targets) do
                if name:find(target:lower()) then
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    local part = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
                    if part and humanoid and humanoid.Health > 0 then
                        local dist = (hrp.Position - part.Position).Magnitude
                        if dist <= self.Range then
                            return obj, part
                        end
                    end
                end
            end
        end
    end
    return nil
end

function NPCWarp:Warp(npc, part)
    if not npc or not part then return end
    if not LocalPlayer.Character then return end
    
    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    
    -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏î‡πâ‡∏≤‡∏ô‡∏´‡∏ô‡πâ‡∏≤ 4 studs
    pcall(function()
        hrp.CFrame = CFrame.new(part.Position) * CFrame.new(0, 0, -4) + Vector3.new(0, 2, 0)
        hrp.Velocity = Vector3.new(0, 0, 0)
    end)
    
    -- Auto Interact
    if self.Mode == "Interact" then
        local prompt = npc:FindFirstChildOfClass("ProximityPrompt")
        if prompt then
            pcall(function()
                prompt:InputHoldBegin()
                task.wait(0.1)
                prompt:InputHoldEnd()
            end)
        end
        -- ClickDetector
        local click = npc:FindFirstChildOfClass("ClickDetector")
        if click and fireclickdetector then
            fireclickdetector(click)
        end
    elseif self.Mode == "Attack" then
        -- ‡πÉ‡∏ä‡πâ Tool ‡∏ï‡∏µ
        local tool = LocalPlayer.Character:FindFirstChildOfClass("Tool")
        if tool then
            tool:Activate()
        end
    end
end

function NPCWarp:Update()
    if not self.Enabled then return end
    
    -- ‡∏™‡πÅ‡∏Å‡∏ô‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    if tick() - self.LastScan < 1 then return end
    self.LastScan = tick()
    
    local npc, part = self:Find()
    if npc and part then
        local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            local dist = (hrp.Position - part.Position).Magnitude
            if dist > 10 then
                self:Warp(npc, part)
            else
                -- ‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡πÅ‡∏•‡πâ‡∏ß ‡∏ó‡∏≥ Action
                if self.Mode == "Interact" then
                    local prompt = npc:FindFirstChildOfClass("ProximityPrompt")
                    if prompt then
                        prompt:InputHoldBegin()
                        task.wait(0.1)
                        prompt:InputHoldEnd()
                    end
                elseif self.Mode == "Attack" then
                    local tool = LocalPlayer.Character:FindFirstChildOfClass("Tool")
                    if tool then tool:Activate() end
                end
            end
        end
    end
end

function NPCWarp:Toggle()
    self.Enabled = not self.Enabled
    
    if self.Enabled then
        print("[NPC] ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô (" .. self.Mode .. ")")
        self.Connection = game:GetService("RunService").Heartbeat:Connect(function()
            self:Update()
        end)
    else
        print("[NPC] ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô")
        if self.Connection then
            self.Connection:Disconnect()
            self.Connection = nil
        end
    end
    
    return self.Enabled
end

-- UI (LayoutOrder 13)
local NPCFrame = Instance.new("Frame")
NPCFrame.Size = UDim2.new(1, -30, 0, 130)
NPCFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
